{
  "name": "repos-using-electron",
  "version": "1.6.0",
  "description": "A collection of all public repositories on GitHub that depend on `electron-prebuilt`",
  "repository": "https://github.com/electron/repos-using-electron",
  "main": "index.js",
  "scripts": {
    "start": "npm run discover",
    "test": "mocha && standard && standard-markdown",
    "collectors": "npm-run-all collectors:*",
    "collectors:repo-names": "node lib/collectors/repo-names.js",
    "collectors:repo-data": "node lib/collectors/repo-data.js",
    "collectors:package-json": "node lib/collectors/package-json.js",
    "collectors:contributors": "node lib/collectors/contributors.js",
    "reports": "npm-run-all reports:*",
    "reports:contributors": "node reports/contributors.js > reports/contributors.md",
    "reports:deps": "node reports/dependencies.js > reports/dependencies.md",
    "reports:forks": "node reports/forks.js > reports/forks.md",
    "reports:electron-builder-configurations": "node reports/electron-builder-configurations.js > reports/electron-builder-configurations.md"
  },
  "keywords": [
    "electron",
    "github",
    "community"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "count-array-values": "^1.1.0",
    "dedent": "^0.6.0",
    "dependent-packages": "^1.0.0",
    "dotenv": "^2.0.0",
    "fetch-nice-package": "^1.0.1",
    "get-repo-package-json": "^1.0.0",
    "keyed-array": "^2.1.0",
    "limiter": "^1.1.0",
    "lodash.flatten": "^4.4.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.1.0",
    "octokat": "^0.4.18",
    "path-exists": "^3.0.0",
    "pify": "^2.3.0",
    "request": "^2.74.0",
    "standard": "^8.0.0-beta.5",
    "standard-markdown": "^1.2.1"
  },
  "dependencies": {
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "nice-package": "^1.2.0",
    "object-values": "^1.0.0",
    "require-dir": "^0.3.0"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
