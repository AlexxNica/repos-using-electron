  "packageJSON": {
    "name": "fluidity-browser",
    "version": "1.0.0",
    "description": "Browser component of Fluidity.",
    "main": "main.js",
    "scripts": {
      "build": "npm run build:app && npm run build:browser && node scripts/copy_files.js",
      "build:app": "NODE_ENV=production webpack --config config/webpack.app.babel.js",
      "build:browser": "NODE_ENV=production BABEL_ENV=production webpack --config config/webpack.browser.babel.js",
      "lint": "eslint . --ext .js,.jsx",
      "package": "electron-packager . Fluidity --platform=darwin --arch=x64 --version=0.36.8 --out=dist",
      "start:dev:app": "babel-node scripts/dev_app.js",
      "start:dev:browser": "babel-node scripts/dev_server.js",
      "start": "babel-node scripts/development.js",
      "start:production": "npm run build && NODE_ENV=production electron .",
      "test": "npm run lint && npm run test:app && npm run test:browser",
      "test:app": "NODE_ENV=test electron-mocha spec/app --compilers js:babel-core/register",
      "test:browser": "NODE_ENV=test karma start",
      "test:watch": "npm run test:watch:browser",
      "test:watch:browser": "npm run test:browser -- --auto-watch --no-single-run"
    },
