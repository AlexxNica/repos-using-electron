      "_install": "npm i --prune && coffee test/init.coffee",
      "watch": "coffee ./watch.coffee",
      "compile": "coffee -e \"(require('./modules/am-compiler/Base.coffee'))::compile()\"",
      "electron": "electron ./electron/.build/start.js"
