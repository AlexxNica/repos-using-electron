    "scripts": {
      "prelint": "rm -rf now.sh-darwin-x64",
      "lint": "eslint app/** ui/**",
      "pretest": "npm run lint",
      "test": "nyc ava",
      "posttest": "npm run report",
      "report": "nyc report --reporter=html",
      "start": "NODE_ENV=development DEBUG=* electron .",
      "prebuild": "npm run test",
      "build": "electron-packager . --platform=darwin --arch=x64 --version=1.3.3 --ignores=node_modules/electron-* now.sh",
      "postbuild": "npm run copy-icon",
      "copy-icon": "cp icon.icns now.sh-darwin-x64/now.sh.app/Contents/Resources/electron.icns",
      "prepackage": "npm run build",
      "package": "asar pack now.sh-darwin-x64/now.sh.app/Contents/Resources/app now.sh-darwin-x64/now.sh.app/Contents/Resources/app.asar"
