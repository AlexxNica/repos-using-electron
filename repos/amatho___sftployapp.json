    "build": "",
    "package": "electron-packager ./ SFTPloy --platform=darwin,linux,win32 --arch=all --out=dist --prune --asar --ignore=\\.gitignore --overwrite",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build",
    "bump": "echo \"Enter type for npm version and press [ENTER]: \" && read bump_version && cd app && PACKAGE_VERSION=$(npm --no-git-tag-version version $bump_version) && git add package.json && git commit --amend --no-edit && git tag -a $PACKAGE_VERSION -m \"$PACKAGE_VERSION\""
