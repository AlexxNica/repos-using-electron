      "electron": "^1.3.2",
      "electron-packager": "^7.5.1",
      "electron-prebuilt": "^1.2.0",
      "eslint": "3.1.1",
      "eslint-loader": "1.4.1",
      "jest-cli": "^14.0.0"
    },
    "dependencies": {
      "babel-eslint": "^6.1.2",
      "play-sound": "0.0.9",
      "say": "^0.9.0"
    },
