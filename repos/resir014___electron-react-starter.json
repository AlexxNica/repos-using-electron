  "packageJSON": {
    "name": "electron-react-starter",
    "version": "0.1.0",
    "description": "",
    "main": "./init.js",
    "scripts": {
      "dev": "npm run build:dev && npm run start:dev",
      "build:dev": "npm run clean:dist && cross-env NODE_ENV=development webpack",
      "build:prod": "npm run clean:dist && cross-env NODE_ENV=production webpack",
      "clean": "npm run clean:dist",
      "clean:dist": "rimraf ./build",
      "clean:pkg": "rimraf ./package",
      "package": "npm run build:prod && npm run package:win",
      "package:win": "cross-env NODE_ENV=production electron-packager . Counter --prune --platform=win32 --out=./package",
      "start": "npm run build:prod && cross-env NODE_ENV=production electron .",
      "start:dev": "cross-env NODE_ENV=development electron .",
      "test": "npm run test:lint && npm run test:mocha",
      "test:lint": "cross-env NODE_ENV=test standard",
      "test:mocha": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register --require ignore-styles",
      "watch": "cross-env NODE_ENV=development webpack-dev-server"
    },
    "author": "",
    "license": "MIT",
