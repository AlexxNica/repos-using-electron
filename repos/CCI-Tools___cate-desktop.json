  "packageJSON": {
    "name": "cate-desktop",
    "productName": "Cate",
    "version": "0.0.1",
    "description": "ESA CCI Toolbox (Cate)",
    "private": true,
    "scripts": {
      "start": "electron app",
      "test:e2e": "mocha e2e/**/*-test.js",
      "pretest": "npm run compile",
      "test": "mocha app/main/**/*.spec.js app/renderer/**/*.spec.js",
      "postinstall": "install-app-deps",
      "compile": "tsc",
      "compile:trace": "tsc --traceResolution",
      "dist": "build --x64 --publish never",
      "pack": "build --x64 --publish never --dir",
      "clean": "rimraf app/main app/renderer dist",
      "clean:all": "npm run clean && rimraf node_modules app/node_modules"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/CCI-Tools/cate-desktop.git"
    },
    "keywords": [
      "esa",
      "cci",
      "toolbox",
      "climate",
      "science"
    ],
    "author": "Norman Fomferra (Brockmann Consult GmbH)",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/CCI-Tools/cate-desktop/issues"
    },
    "homepage": "https://github.com/CCI-Tools/cate-desktop",
    "devDependencies": {
      "@types/electron": "^1.4.26",
      "@types/mocha": "^2.2.32",
      "@types/react": "^0.14.46",
      "@types/react-dom": "^0.14.18",
      "@types/react-redux": "^4.4.32",
      "@types/redux": "^3.6.31",
      "chai": "^3.5.0",
      "chai-as-promised": "^6.0.0",
      "electron-builder": "latest",
      "electron-prebuilt": "1.4.5",
      "mocha": "^3.1.2",
      "react": "^15.3.2",
      "react-dom": "^15.3.2",
      "react-redux": "^4.4.5",
      "redux": "^3.6.0",
      "redux-log": "^2.0.0",
      "rimraf": "^2.5.4",
      "spectron": "^3.4.0",
      "typescript": "^2.0.8"
    },
    "build": {
      "appId": "org.esa.ccitools.Cate",
      "app-category-type": "app.category.climate-science",
      "win": {
        "target": [
          "nsis",
          "zip"
        ],
        "icon": "app/resources/win32/cate-icon.ico"
      },
      "nsis": {
        "oneClick": true,
        "installerHeaderIcon": "app/resources/win32/cate-icon.ico"
      },
      "mac": {
        "icon": "app/resources/darwin/icon.icns"
      },
      "dmg": {
        "icon": "app/resources/darwin/icon.icns",
        "background": "resources/darwin/dmg-background.png"
      }
    }
  },
