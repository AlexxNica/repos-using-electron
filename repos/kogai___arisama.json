    "scripts": {
      "init": "$(npm bin)/webdriver-manager update && chmod +xxx ./package.sh",
      "start": "$(npm bin)/electron .",
      "package": "./package.sh",
      "package:minor": "npm version minor && npm run package",
      "package:patch": "npm version patch && npm run package",
      "set:electron:osx": "export ELECTRON_PATH=./release/SlackStatus-darwin-x64/SlackStatus.app/Contents/MacOS/Electron",
      "set:electron:linux": "export ELECTRON_PATH=./release/SlackStatus-linux-x64/SlackStatus",
      "test": "npm run set:electron:linux && npm run test:mocha && npm run test:e2e:linux",
      "test:all": "npm run set:electron:osx && npm run test:mocha && npm run test:e2e:osx",
      "test:mocha": "npm run set:electron:osx && $(npm bin)/electron-mocha --recursive -r espower-babel/guess --renderer",
      "test:unit": "npm run set:electron:osx && $(npm bin)/electron-mocha -r espower-babel/guess --renderer",
      "test:e2e:osx": "NODE_ENV=test $(npm bin)/nightwatch -e chrome",
      "test:e2e:linux": "NODE_ENV=test $(npm bin)/nightwatch -e electron",
      "test:e2e:unit": "NODE_ENV=test $(npm bin)/nightwatch -e chrome -t",
      "server:start": "$(npm bin)/forever start authServer/entry.js",
      "server:stop": "$(npm bin)/forever stopall",
      "server:restart": "$(npm bin)/forever restartall",
      "server:logs": "$(npm bin)/forever logs",
      "server:list": "$(npm bin)/forever list",
      "install:native": "HOME=~/.electron-gyp npm install -S"
    },
