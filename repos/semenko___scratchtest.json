      "react-router": "=0.13.3",
      "react-tools": "=0.13.3",
      "reactify": "=1.1.1",
      "reflux": "=0.2.7",
      "reloading": "0.0.6",
      "superagent": "=1.2.0",
      "underscore": "=1.8.3",
      "watchify": "=3.2.1"
    },
    "devDependencies": {
      "electron-packager": "=4.1.2",
      "electron-prebuilt": "=0.27.2",
      "esprima-fb": "=15001.1.0-dev-harmony-fb",
