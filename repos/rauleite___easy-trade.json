  "packageJSON": {
    "name": "pkg-easy-trade",
    "productName": "EasyTrade",
    "description": "An EasyTrade project",
    "version": "0.0.1",
    "author": "Raul Leite <raul.abreu.leite@gmail.com>",
    "main": "app/index.js",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/rauleite/easy-trade.git"
    },
    "keywords": [
      "betfair",
      "trade",
      "angularjs"
    ],
    "config": {
      "id": "com.appelgriebsch.electron-boilerplate",
      "ico": "app/assets/boilerplate.ico",
      "icns": "app/assets/boilerplate.icns",
      "png": "app/assets/boilerplate.png"
    },
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/rauleite/easy-trade/issues"
    },
    "homepage": "https://github.com/rauleite/easy-trade#readme",
    "scripts": {
      "postinstall": "npm run app-install",
      "start": "electron --debug=5858 app/",
      "app-install": "cd app && npm install --no-optional",
      "build:linux": "npm run -s package:linux && npm run -s setup:linux",
      "build:osx": "npm run -s package:osx && npm run -s setup:osx",
      "build:win32": "npm run -s package:win32 && npm run -s setup:win32",
      "package:linux": "npm run -s package:linux-ia32 && npm run -s package:linux-x64",
      "package:linux-ia32": "electron-packager app/ $npm_package_productName --arch ia32 --platform linux --app-version $npm_package_version --app-copyright \"Copyright (c) 2016 $npm_package_author_name\" --asar --icon $npm_package_config_png --prune --out build/",
      "package:linux-x64": "electron-packager app/ $npm_package_productName --arch x64 --platform linux --app-version $npm_package_version --app-copyright \"Copyright (c) 2016 $npm_package_author_name\" --asar --icon $npm_package_config_png --prune --out build/",
      "package:osx": "npm run -s package:osx-app && npm run -s package:osx-mas",
      "package:osx-app": "electron-packager app/ $npm_package_productName --arch x64 --platform darwin --app-version $npm_package_version --app-copyright \"Copyright (c) 2016 $npm_package_author_name\" --app-bundle-id $npm_package_config_id --asar --icon $npm_package_config_png --prune --out build/",
      "package:osx-mas": "electron-packager app/ $npm_package_productName --arch x64 --platform mas --app-version $npm_package_version --app-copyright \"Copyright (c) 2016 $npm_package_author_name\" --app-bundle-id $npm_package_config_id --asar --icon $npm_package_config_png --prune --out build/",
      "package:win32": "npm run -s package:win32-ia32 && npm run -s package:win32-x64",
      "package:win32-ia32": "electron-packager app/ $npm_package_productName --arch ia32 --platform win32 --app-version $npm_package_version --app-bundle-id $npm_package_config_id --app-copyright \"Copyright (c) 2016 $npm_package_author_name$\" --version-string.CompanyName=\"$npm_package_author_name$\" --version-string.ProductName=\"$npm_package_productName$\" --version-string.FileDescription=\"$npm_package_description$\" --asar --icon $npm_package_config_ico --prune --out build/",
      "package:win32-x64": "electron-packager app/ $npm_package_productName --arch x64 --platform win32 --app-version $npm_package_version --app-bundle-id $npm_package_config_id --app-copyright \"Copyright (c) 2016 $npm_package_author_name$\" --version-string.CompanyName=\"$npm_package_author_name$\" --version-string.ProductName=\"$npm_package_productName$\" --version-string.FileDescription=\"$npm_package_description$\" --asar --icon $npm_package_config_ico --prune --out build/",
      "setup:linux": "npm run -s setup:linux-ia32 && npm run -s setup:linux-x64",
      "setup:linux-ia32": "electron-installer-debian --src build/EasyTrade-linux-ia32/ --dest dist/ --arch ia32 --config package-linux.json && electron-installer-redhat --src build/EasyTrade-linux-ia32/ --dest dist/ --arch ia32 --config package-linux.json",
      "setup:linux-x64": "electron-installer-debian --src build/EasyTrade-linux-x64/ --dest dist/ --arch amd64 --config package-linux.json && electron-installer-redhat --src build/EasyTrade-linux-x64/ --dest dist/ --arch amd64 --config package-linux.json",
      "setup:osx": "npm run -s setup:osx-dmg && npm run -s setup:osx-mas",
      "setup:osx-dmg": "appdmg package-osx.json dist/EasyTrade-standalone-$npm_package_version.dmg",
      "setup:osx-mas": "appdmg package-osx.json dist/EasyTrade-mas-$npm_package_version.dmg",
      "setup:win32": "npm run -s setup:win32-ia32 && npm run -s setup:win32-x64",
      "setup:win32-ia32": "electron-installer-windows --src build/EasyTrade-win32-ia32/ --dest dist/win32 --arch ia32 --config package-win32.json",
      "setup:win32-x64": "electron-installer-windows --src build/EasyTrade-win32-x64/ --dest dist/win64 --arch amd64 --config package-win32.json"
    },
    "dependencies": {
      "devtron": "^1.1.0",
      "electron-packager": "^7.0.0",
      "electron": "^1.3.0",
      "spectron": "^3.0.0"
    },
    "optionalDependencies": {
      "appdmg": "^0.4.5",
      "electron-installer-debian": "^0.3.0",
      "electron-installer-redhat": "^0.3.0",
      "electron-installer-windows": "^0.2.0"
    }
  },
