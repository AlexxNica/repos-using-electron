  "packageJSON": {
    "name": "unique-media-explorer",
    "version": "0.0.1",
    "description": "Unique Media Explorer (UME)",
    "scripts": {
      "build-electron": "shx rm -rf bin/electron/src/ && shx mkdir -p bin/electron/src/ && shx cp -r electron bin/electron/src/ && shx cp -r shared bin/electron/src/",
      "move-package-config": "shx mv bin/electron/src/electron/package.json bin/electron/src/ && node scripts/change-electron-main.js",
      "start": "electron electron/",
      "predeploy-win32-x64": "npm run build-electron && npm run move-package-config",
      "deploy-win32-x64": "electron-packager bin/electron/src/ UniqueMediaExplorer --platform=win32 --arch=x64 --out bin/electron/ --overwrite --asar"
    },
