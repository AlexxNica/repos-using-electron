  "packageJSON": {
    "name": "animation-image-converter",
    "version": "1.2.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "build:app": "cd app && npm run build",
      "clean:app": "del tmp-release-* アニメ画像に変換する君* AnimationImageConverter*",
      "copy:release:app": "copyfiles  -u 1 ./app/bundle.js ./app/index.html ./app/main.js ./app/styles/**/*.css ./app/src/**/*.html ./app/imgs/**/*.* ./resources/package.json ./app/js/**/*.js ./app/styles/**/*.*js ./app/node_modules/bootstrap/dist/js/bootstrap.js ./app/node_modules/bootstrap/dist/css/bootstrap.css ./app/resources/font-awesome-4.6.3/css/*.css ./app/resources/font-awesome-4.6.3/fonts/*.* tmp-release",
      "copy:release:app-mac": "npm run copy:release:app && node-mv tmp-release tmp-release-mac ",
      "copy:release:apngasm-mac": " cp -P ./resources/apngasm-2.9-bin-macosx/apngasm tmp-release-mac/bin/apngasm",
      "copy:release:webp-mac": "cp -P ./resources/libwebp-0.5.0-mac-10.9/bin/cwebp tmp-release-mac/bin/cwebp & cp -P ./resources/libwebp-0.5.0-mac-10.9/bin/webpmux tmp-release-mac/bin/webpmux",
      "copy:release:pngquant-mac": "cp -P ./resources/pngquant-mac/pngquant tmp-release-mac/bin/pngquant",
      "copy:release:bin-mac": "mkdir tmp-release-mac/bin/ && npm run copy:release:webp-mac && npm run copy:release:apngasm-mac & npm run copy:release:pngquant-mac",
      "install:package-mac": "cd tmp-release-mac && npm install --production",
      "package-mac": "electron-packager ./tmp-release-mac 'アニメ画像に変換する君' --platform=darwin --arch=x64 --overwrite --asar --icon='resources/app.icns'  --version='1.2.3'",
      "release-mac": "npm run clean:app && npm run build:app && npm run copy:release:app-mac && npm run copy:release:bin-mac && npm run install:package-mac && npm run package-mac",
      "package-mas": "electron-packager ./tmp-release-mac 'アニメ画像に変換する君' --platform=mas --overwrite  --arch=x64 --app-bundle-id='media.ics.AnimationImageConverter'  --extend-info=dev/Info.plist  --version='1.2.3'  --app-version='1.2.0' --build-version='1.2.000' --icon='resources/app.icns' --overwrite ",
      "sign-mas": "electron-osx-sign 'アニメ画像に変換する君-mas-x64/アニメ画像に変換する君.app' --entitlements='dev/parent.plist' --entitlements-inherit='dev/child.plist'",
      "package-sign-mas": "npm run package-mas && npm run sign-mas",
      "flat": "electron-osx-flat 'アニメ画像に変換する君-mas-x64/アニメ画像に変換する君.app' --identity '3rd Party Mac Developer Installer: ICS INC. (53YCXL8YSM)' --verbose --pkg AnimationImageConverter.pkg",
      "release-mas": "npm run clean:app && npm run build:app && npm run copy:release:app-mac && npm run copy:release:bin-mac && npm run install:package-mac && npm run package-sign-mas && npm run flat",
      "copy:release:app-win32": "npm run copy:release:app && node-mv tmp-release tmp-release-win32 ",
      "copy:release:apngasm-win32": " cp -P ./resources/apngasm-2.9-bin-win32/apngasm.exe tmp-release-win32/bin/apngasm.exe",
      "copy:release:webp-win32": "cp -P ./resources/libwebp-0.5.0-windows-x86/bin/cwebp.exe tmp-release-win32/bin/cwebp.exe & cp -P ./resources/libwebp-0.5.0-windows-x86/bin/webpmux.exe tmp-release-win32/bin/webpmux.exe",
      "copy:release:pngquant-win32": "cp -P ./resources/pngquant-win/pngquant.exe tmp-release-win32/bin/pngquant.exe",
      "copy:release:bin-win32": "mkdir tmp-release-win32/bin/ && npm run copy:release:webp-win32 & npm run copy:release:apngasm-win32 && npm run copy:release:pngquant-win32",
      "package-win32": "cd tmp-release-win32 && npm install && cd ../ && electron-packager ./tmp-release-win32 animation-image-converter --platform=win32 --arch=ia32 --overwrite --asar --icon=resources/app.ico",
      "release-win32": "npm run clean:app && npm run build:app && npm run copy:release:app-win32 && npm run copy:release:bin-win32"
    },
