    "name": "one-more-todo-list",
    "version": "1.0.0",
    "description": "A simple todo list in electron with webpack hot replacement",
    "main": "",
    "scripts": {
      "start-dev": "set NODE_ENV=dev&& electron ./output/server-entry.js",
      "start-production": "set NODE_ENV=production&& electron ./output/server-entry.js",
      "build": "webpack --config webpack.config.js",
      "start-webpack": "webpack-dev-server --hot --inline --config webpack.config.js --content-base output/",
      "dev": "node ./node_modules/concurrently/src/main.js \"npm run start-webpack\" \"npm run start-dev\"",
      "production": "npm run build&& npm run start-production",
      "test": "ava"
    },
