  "packageJSON": {
    "name": "jetta",
    "version": "1.0.1",
    "description": "Multi-platform Bandcamp/Souldcloud Downloader built with Electron.",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "dev": "electron . --debug",
      "dist:win": "build --windows --x64 --dir",
      "dist:mac": "build --mac",
      "installer": "node scripts/installer.js",
      "dist": "npm run dist:win && npm run installer"
    },
    "repository": "https://github.com/dzt/jetta",
    "keywords": [
      "soundcloud",
      "bandcamp"
    ],
    "license": "MIT",
    "homepage": "https://github.com/dzt/jetta#readme",
    "dependencies": {
      "application-config": "^0.2.2",
      "async": "^2.0.1",
      "cheerio": "^0.22.0",
      "cron": "^1.1.0",
      "debug": "^2.2.0",
      "font-awesome": "^4.6.3",
      "jquery": "^3.1.0",
      "path": "^0.12.7",
      "request": "^2.74.0",
      "rimraf": "^2.5.4",
      "url": "^0.11.0"
    },
    "devDependencies": {
      "devtron": "^1.3.0",
      "electron": "^1.3.5",
      "electron-builder": "^6.7.6",
      "electron-installer-windows": "^0.2.0",
      "electron-packager": "^8.0.0",
      "electron-winstaller": "^2.3.4"
    },
    "build": {
      "productName": "Jetta",
      "asar": false,
