    "name": "hosty",
    "productName": "Hosty",
    "version": "0.1.2",
    "description": "Hosts file manager based on Electron",
    "keywords": [
      "electron",
      "react",
      "redux",
      "material-ui",
      "webpack",
      "hosts"
