    "scripts": {
      "electron": "electron .",
      "test:lint": "tslint --force 'src/**/*.ts'",
      "test:lint:fail": "tslint 'src/**/*.ts'",
      "test": "npm run test:lint:fail && npm run ionic:build",
      "clear:all": "rm -rf ./platforms/*-x64",
      "clear:osx": "rm -rf *-darwin-x64; rm -rf ./platforms/*-mas-x64",
      "clear:win": "rm -rf *-win32-ia32; rm -rf ./platforms/*-win32-x64",
      "clear:nix": "rm -rf *-linux-ia32; rm -rf ./platforms/*-linux-x64",
      "pack:osx": "npm run clear:osx && electron-packager ./www/ --platform=darwin,mas --arch=x64 --asar --out=platforms",
      "pack:win": "npm run clear:win && electron-packager ./www/ --platform=win32 --arch=ia32,x64 --asar --out=platforms",
      "pack:nix": "npm run clear:nix && electron-packager ./www/ --platform=linux --arch=ia32,x64 --asar --out=platforms",
      "ionic:build": "ionic-app-scripts build",
      "ionic:build:electron": "npm run ionic:build && cp electron.js www/electron.js && cp package.json www/package.json && npm run server:build",
      "ionic:serve": "ionic-app-scripts serve",
      "start": "npm run ionic:serve",
      "start:server": "nodemon --ignore \"*.json\" --exec \"ts-node\" -- \"src/server/index.ts\"",
      "server:build": "tsc -p src/server",
      "migrate:latest": "knex migrate:latest --knexfile src/server/knexfile.ts",
      "migrate:rollback": "knex migrate:rollback --knexfile src/server/knexfile.ts",
      "migrate:reset": "npm run migrate:rollback && npm run migrate:latest",
      "seed:run": "knex seed:run --knexfile src/server/knexfile.ts"
    },
    "main": "electron.js",
    "config": {
      "ionic_src_dir": "src/client"
    },
