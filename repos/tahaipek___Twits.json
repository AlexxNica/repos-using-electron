{
  "name": "twits",
  "author": "Taha İPEK",
  "description": "Yakınımdaki Twitler",
  "version": "1.0.5",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build-64 && npm run build-win32 && npm run build-linux32",
    "build-64": "npm run build-mac && npm run build-win && npm run build-linux",
    "build-mac": "electron-packager ./ Twits --app-bundle-id=twits --icon=images/appicon.icns --asar --prune --out=build --overwrite=true --platform=darwin --arch=x64 --version=1.2.5 --app-version=1.0.5",
    "build-win": "electron-packager ./ Twits --app-bundle-id=twits --version-string.FileDescription=Twits --icon=assets/win/logo.ico --asar --prune --out=build --overwrite=true --platform=win32 --arch=x64 --version=1.2.5 --app-version=1.0.5",
    "build-win32": "electron-packager ./ Twits --app-bundle-id=twits --asar --prune --version-string.FileDescription=Twits --icon=assets/win/logo.ico --out=build --overwrite=true --platform=win32 --arch=ia32 --version=1.2.5 --app-version=1.0.5",
    "build-linux": "electron-packager ./ Twits --app-bundle-id=twits --asar --prune --out=build --overwrite=true --platform=linux --arch=x64 --version=1.2.5 --app-version=1.0.5",
    "build-linux32": "electron-packager ./ Twits --app-bundle-id=twits --asar --prune --out=build --overwrite=true --platform=linux --arch=ia32 --version=1.2.5 --app-version=1.0.5"
  },
