  "packageJSON": {
    "name": "dat-desktop",
    "productName": "Dat Desktop",
    "version": "1.0.1",
    "license": "MIT",
    "repository": "juliangruber/dat-desktop",
    "description": "Dat Desktop App",
    "scripts": {
      "rebuild": "cd app && npm rebuild --runtime=electron --target=1.4.4 --disturl=https://atom.io/download/atom-shell --abi=50",
      "build-css": "node-sass --importer node_modules/node-sass-magic-importer --include-path app/scss app/scss/main.scss app/public/css/main.css",
      "watch-css": "nodemon -e scss -x \"npm run build-css\"",
      "start": "NODE_ENV=development electron app",
      "test": "standard app"
    },
    "devDependencies": {
      "appdmg": "^0.4.5",
      "bulkify": "^1.4.2",
      "dat-design": "^2.0.0",
      "dat-icons": "^1.3.0",
      "dependency-check": "^2.6.0",
      "electron": "1.4.4",
      "electron-packager": "^7.0.2",
      "node-sass": "^3.8.0",
      "node-sass-magic-importer": "^1.0.1",
      "nodemon": "^1.9.2",
      "standard": "^7.1.2"
    }
  },
