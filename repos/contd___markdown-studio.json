  "scripts": {
    "start": "electron .",
    "asar": "asar pack markdown-studio app.asar",
    "clean": "rimraf dist",
    "exe32": "electron-packager . markdown-studio --platform linux --arch ia32 --out dist/ --ignore \"(dist|node_modules/electron.*)\"",
    "exe64": "electron-packager . markdown-studio --platform linux --arch x64 --out dist/ --ignore \"(dist|node_modules/electron.*)\"",
    "deb32": "electron-installer-debian --src dist/markdown-studio-linux-ia32/ --arch i386 --config config.json",
    "deb64": "electron-installer-debian --src dist/markdown-studio-linux-x64/ --arch amd64 --config config.json",
    "build": "npm run clean && npm run exe32 && npm run deb32 && npm run exe64 && npm run deb64"
  },
  "build": {
    "appId": "markdown-studio-jpk",
    "app-category-type": "Utility",
    "win": {
      "iconUrl": "resources/Icon.png"
    }
  },
  "devDependencies": {
