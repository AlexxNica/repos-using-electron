  "packageJSON": {
    "name": "farma-mobile-native-dashboard",
    "productName": "Farma Mobile",
    "version": "1.2.0",
    "description": "Official Farma Mobile dashboard app",
    "license": "MIT",
    "repository": "farma-mobile/farma-mobile-native-dashboard",
