    "devDependencies": {
      "electron": "^1.4.5",
      "electron-installer-debian": "^0.4.0",
      "electron-installer-windows": "^0.2.0",
      "electron-packager": "^8.2.0",
      "electron-rebuild": "^1.3.0"
    },
    "dependencies": {
      "mpris-service": "^1.1.1"
    }
