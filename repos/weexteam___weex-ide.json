    "license": "CC0-1.0",
    "devDependencies": {
      "electron-installer-dmg": "^0.1.1",
      "electron-packager": "^7.0.4",
      "electron-prebuilt": "^1.2.0",
      "weex-html5": "^0.2.18"
    },
    "dependencies": {
      "codemirror": "^5.16.0",
