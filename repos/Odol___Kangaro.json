    "scripts": {
      "task": "cross-env -NODE_ENV='development' gulp --require babel-register",
      "renderer-dev": "cross-env -NODE_ENV='development' node -r babel-register devserver.js",
      "renderer-prod": "cross-env -NODE_ENV='production' node -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors --config webpack.renderer.prod.config.js -w",
      "main-dev": "cross-env -NODE_ENV='development' node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.main.dev.config.js -w",
      "main-prod": "cross-env -NODE_ENV='production' node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.main.prod.config.js -w",
      "dev": "clear & npm run tasks & npm run renderer-dev & npm run main-dev",
      "run-dev": "export NODE_ENV=development & electron bin/kangaro-dev --enable-transparent-visuals",
      "run-prod": "export NODE_ENV=production & electron bin/kangaro",
      "gyp": "node-gyp rebuild --target=1.4.1 --arch=x64 --dist-url=https://atom.io/download/atom-shell"
