    "karma-ie-launcher": "^1.0.0",
    "karma-mocha-reporter": "^2.0.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-tap": "^2.0.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.2",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.2",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  },
