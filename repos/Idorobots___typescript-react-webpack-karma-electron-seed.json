  "packageJSON": {
    "name": "typescript-react-webpack-karma-electron-seed",
    "version": "0.1.0",
    "description": "A seed project for full-featured TypeScript 2.0 + React + Webpack + Karma + Electron stack",
    "author": "Kajetan Rzepecki <kajtek@idorobots.org>",
    "license": "MIT",
    "main": "dist/app.js",
    "types": "dist/src/main.d.ts",
    "scripts": {
      "lint": "npm run lint-css && npm run lint-ts",
      "lint-css": "stylelint --config './stylelint.json' './src/**/*.css'",
      "lint-ts": "tslint 'src/**/*.tsx' 'src/**/*.ts'",
      "test": "cross-env NODE_ENV=test karma start --singleRun true",
      "test:ci": "cross-env NODE_ENV=test karma start --singleRun true --browsers 'PhantomJS'",
      "test:watch": "cross-env NODE_ENV=test karma start --singleRun false",
      "build": "cross-env NODE_ENV=production webpack --bail --colors",
      "build:watch": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --colors dist/",
      "start": "electron dist/app.js",
      "package-app": "electron-packager . app",
      "prebuild": "npm run test",
      "pretest": "npm run lint",
      "posttest": "istanbul check-coverage --statement 80 --branch 50 --function 70",
      "clean": "rimraf dist node_modules"
    },
    "dependencies": {
      "es6-shim": "^0.35.1",
