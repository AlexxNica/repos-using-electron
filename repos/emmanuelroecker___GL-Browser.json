    "electron-builder": "^5.0.2",
    "electron-mocha": "^1.2.1",
    "electron-prebuilt": "^1.0.0",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.3",
    "rimraf": "^2.5.2"
  },
