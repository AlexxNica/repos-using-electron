  "packageJSON": {
    "name": "PixivRobot",
    "version": "2.0.3",
    "main": "./main/index.js",
    "scripts": {
      "prod": "npm run build && npm run pack:osx",
      "pack:osx": "cp ./package.json ./build && electron-packager ./build PixivRobot --platform=darwin --arch=x64 --out=./prod --overwrite",
      "pack:win": "cp ./package.json ./build && electron-packager ./build PixivRobot --platform=win32 --arch=x64 --out=./prod --overwrite",
      "build": "npm run build:renderer && npm run build:main",
      "build:renderer": "NODE_ENV=production webpack --profile --progress --config ./webpack.config.renderer.js",
      "build:main": "NODE_ENV=production webpack --config ./webpack.config.main.js",
      "dev": "npm run dev:renderer & npm run dev:main",
      "dev:renderer": "NODE_ENV=development webpack-dev-server --hot --inline --color --host 0.0.0.0 --port 3000 --config ./webpack.config.renderer.js",
      "dev:main": "NODE_ENV=development electron -r babel-register ./src/main/index.js",
      "test": "mocha --compilers js:babel-core/register -R spec ",
      "test:watch": "mocha --compilers js:babel-core/register -R spec --watch",
      "test:cover": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha"
    },
