      "start": "electron ./app/browser.js",
      "dev": "electron . --debug",
      "pack-mac": "electron-packager ./app hot-app --asar --asar-unpack-dir=tools  --overwrite --platform=x64 --arch=ia32 --icon=app/assets/images/icpn.ico --prune=true --out=out",
