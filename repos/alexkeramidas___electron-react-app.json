    "scripts": {
      "start": "electron main.js",
      "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
      "delete-folders": "node delete-folders.js",
      "pack:win": "electron-packager . LarisaBusinessGuide --app-version=%npm_package_version% --out=builds --ignore='^/builds$' --platform=win32 --arch=ia32 --version-string.CompanyName=%npm_package_productName% --version-string.LegalCopyright='Copyright 2016' --version-string.FileDescription=%npm_package_productName% --version-string.OriginalFilename='LarisaBusinessGuide.exe' --version-string.InternalName%npm_package_productName% --version-string.ProductName=%npm_package_productName% --version-string.ProductVersion==%npm_package_version% --asar=true --icon=appres/appIcon.ico --overwrite",
      "build-installer": "grunt create-windows-installer"
    },
    "author": "Alex G. Keramidas",
    "license": "MIT",
    "dependencies": {
      "babel-preset-es2015": "^6.14.0",
      "babel-preset-react": "^6.11.1",
      "babelify": "^7.3.0",
      "classnames": "^2.2.5",
