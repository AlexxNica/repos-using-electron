    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
      "start": "electron main.js",
      "pack:win32": "electron-packager . Boxy --overwrite --platform=win32 --arch=ia32 --version=0.36.7 --icon=assets/boxy.ico --out=dist  --prune --ignore=dist",
