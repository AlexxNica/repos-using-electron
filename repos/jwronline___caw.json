  "packageJSON": {
    "name": "jwr-caw",
    "version": "0.1.4",
    "description": "A caution and warning system for the JWR shuttle",
    "main": "app.js",
    "scripts": {
      "watch": "webpack --watch --progress --hide-modules",
      "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
      "start": "npm run build && electron app.js",
      "pack": "npm run build && electron-packager . --platform=linux,darwin,win32 --arch=x64 --overwrite",
      "test": "xo"
    },
    "repository": "https://github.com/jwronline/caw",
    "license": "Apache-2.0",
    "dependencies": {
      "dialog-polyfill": "^0.4.4",
      "electron": "^1.4.5",
      "electron-context-menu": "^0.7.0",
      "express": "^4.14.0",
      "opn": "^4.0.2",
      "pug": "^2.0.0-beta6",
      "socket.io": "^1.5.1"
    },
    "devDependencies": {
      "babel-core": "^6.18.0",
      "babel-loader": "^6.2.7",
      "babel-preset-es2015": "^6.18.0",
      "cross-env": "^3.1.3",
      "css-loader": "^0.25.0",
      "electron-packager": "^8.2.0",
      "extract-text-webpack-plugin": "^1.0.1",
      "file-loader": "^0.9.0",
      "json-loader": "^0.5.4",
      "node-sass": "^3.10.1",
      "sass-loader": "^4.0.2",
      "socket.io-client": "^1.5.1",
      "strip-loader": "^0.1.2",
      "style-loader": "^0.13.1",
      "webpack": "^1.13.3",
      "xo": "^0.17.0"
    },
    "xo": {
      "space": true,
      "overrides": [
        {
          "files": "src/js/**.js",
          "envs": [
            "browser"
          ]
        }
      ]
    }
  },
