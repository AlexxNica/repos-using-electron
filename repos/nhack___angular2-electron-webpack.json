  "packageJSON": {
    "name": "angular2-electron-webpack",
    "version": "0.0.1",
    "description": "A webpack starter for angular 2",
    "scripts": {
      "start": "webpack-dev-server --inline --progress --port 8080",
      "test": "karma start",
      "clean": "rm -rf ./dist",
      "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
      "postinstall": "typings install",
      "electron-watch": "webpack --watch --config config/webpack.electron.js --progress --profile --colors --display-error-details --display-cached",
      "electron-start": "electron dist",
      "electron-build": "rimraf dist && webpack --config config/webpack.electron.prod.js --progress --profile --bail",
      "electron-pack": "npm run clean && npm run electron-build && electron-packager ./dist --out=dist/osx --platform=darwin --arch=x64 --no-prune"
    },
    "license": "MIT",
