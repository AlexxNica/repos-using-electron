  "packageJSON": {
    "name": "theclassic-dev",
    "productName": "TheClassic",
    "version": "1.0.0",
    "description": "An extensible music player",
    "main": "src/main-process/main.js",
    "bin": {
      "electron": "./node_modules/.bin/electron",
      "webpack": "./node_modules/.bin/webpack"
    },
    "scripts": {
      "build:all": "npm run prepackage && build -mwl",
      "build:lin": "npm run prepackage && build --linux",
      "build:mac": "npm run prepackage && build --mac",
      "build:test": "npm run prepackage && build --dir",
      "build:win": "npm run prepackage && build --win",
      "clean": "",
      "dev": "concurrently -k \"npm run server\" \"npm run start\"",
      "lint:all": "npm run lint:sass && npm run lint:ts",
      "lint:sass": "cls",
      "lint:ts": "tslint \"src/**/*.ts?(x)\"",
      "package": "npm run build:test && tar -xcf builds.tar.gz dist/",
      "prepackage": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
      "start": "cross-env NODE_ENV=development electron app",
      "setup": "typings install",
      "server": "node server.js"
    },
    "keywords": [
      "music player",
      "music",
      "player",
      "foobar2000",
      "iTunes",
      "winamp"
    ],
    "license": "Apache-2.0",
    "build": {
      "appId": "de.zerotask.theclassic",
      "dmg": {
        "contents": [
          {
            "x": 410,
            "y": 150,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      },
      "mac": {},
      "linux": {
        "target": [
          "deb",
          "AppImage"
        ]
      },
      "win": {
        "target": [
          "squirrel"
        ]
      }
    },
    "devDependencies": {
      "@types/node": "^6.0.57",
      "@types/react": "^0.14.55",
      "@types/react-dom": "^0.14.20",
      "@types/react-redux": "^4.4.35",
