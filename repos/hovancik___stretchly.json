    "devDependencies": {
      "chai": "^3.5.0",
      "chai-as-promised": "^6.0.0",
      "electron": "^1.4.3",
      "electron-builder": "^7.12.2",
      "mocha": "^3.1.2",
      "spectron": "^3.4.0"
    },
    "build": {
      "appId": "net.hovancik.stretchly",
      "category": "public.app-category.healthcare-fitness",
      "productName": "stretchly",
      "dmg": {
        "icon": "build/icon.icns",
        "iconSize": 128,
        "contents": [
          {
            "x": 355,
            "y": 125,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 155,
            "y": 125,
            "type": "file"
          }
        ]
      },
      "linux": {
        "target": [
          "deb",
          "rpm",
          "sh",
          "freebsd",
          "pacman",
          "tar.xz",
          "tar.gz"
        ]
      },
      "win": {
        "target": [
          "squirrel",
          "nsis",
          "7z",
          "zip"
        ],
        "msi": true
      }
    },
    "standard": {
      "globals": [
        "it",
        "describe",
        "beforeEach",
        "afterEach",
        "Audio",
        "fetch",
        "Notification"
      ]
    }
