    "name": "electron-quick-start",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "app/main.js",
    "scripts": {
      "start": "concurrently -k 'npm run start-webpack' 'npm run start-electron'",
      "start-electron": "electron .",
      "start-webpack": "webpack-dev-server --public localhost:8080 --port 8080 --host 0.0.0.0 --history-api-fallback --content-base ./app",
      "build": "./bin/build.sh",
      "postinstall": "rm -rf typings && typings install",
      "test": "karma start",
      "posttest": "npm run coverage",
      "coverage": "npm run coverage:remap && npm run coverage:report",
      "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma.entry.ts",
      "coverage:report": "istanbul report"
    },
