      "grunt-contrib-concat": "^1.0",
      "grunt-contrib-connect": "^1.0",
      "grunt-contrib-copy": "^1.0",
      "grunt-contrib-cssmin": "^1.0",
      "grunt-contrib-htmlmin": "^1.0",
      "grunt-contrib-imagemin": "^1.0",
      "grunt-contrib-jshint": "^1.0",
      "grunt-contrib-uglify": "^1.0",
      "grunt-contrib-watch": "^1.0",
      "grunt-filerev": "^2.1",
      "grunt-google-cdn": "^0.4",
      "grunt-injector": "^1.0",
      "grunt-jscs": "^1.8",
      "grunt-merge-json-xduplicate": "^1.0.1",
      "grunt-newer": "^1.1",
      "grunt-ng-annotate": "^1.0",
      "grunt-postcss": "^0.8",
      "grunt-release": "*",
      "grunt-run": "*",
      "grunt-svgmin": "^2.0",
      "grunt-usemin": "^3.0",
      "grunt-wiredep": "^2.0",
      "innosetup-compiler": "*",
      "jasmine-core": "^2.4",
      "jit-grunt": "^0.10",
      "js-obfuscator": "*",
      "jshint-stylish": "^1.0",
      "karma": "^1.1",
      "karma-jasmine": "^1.0",
      "karma-phantomjs-launcher": "^1.0",
      "phantomjs-prebuilt": "^2.1",
      "q": "^1.*",
      "serve-static": "^1.*",
      "time-grunt": "^1.*"
    },
    "engines": {
      "node": ">=0.10.0"
    },
    "electronOut": "dist-electron",
    "scripts": {
      "start": "./node_modules/.bin/grunt serve",
      "start-dist": "npm run grunt-build && ./node_modules/.bin/electron dist",
      "grunt-build": "npm run grunt-dist && npm run grunt-min",
      "grunt-dist": "./node_modules/.bin/grunt build",
      "grunt-min": "./node_modules/.bin/grunt min",
      "pack": "./node_modules/.bin/electron-packager ./dist $npm_package_name --app-version=$npm_package_version --out=$npm_package_electronOut --prune --asar --platform=linux --arch=x64 --version=$npm_package_electronVersion --overwrite",
      "pack-win-linux": "./node_modules/.bin/electron-packager ./dist $npm_package_name --app-version=$npm_package_version --out=$npm_package_electronOut --prune --asar --platform=win32 --arch=x64 --version=$npm_package_electronVersion --version-string.OriginalFilename=$npm_package_name --version-string.InternalName=$npm_package_name --version-string.ProductName=$npm_package_name --version-string.ProductVersion=$npm_package_version --icon=./app/favicon.ico --overwrite",
      "pack-win": "./node_modules/.bin/electron-packager ./dist %npm_package_name% --app-version=%npm_package_version% --out=%npm_package_electronOut% --prune --asar --platform=win32 --arch=x64 --version=%npm_package_electronVersion% --version-string.OriginalFilename=%npm_package_name% --version-string.InternalName=%npm_package_name% --version-string.ProductName=%npm_package_name% --version-string.ProductVersion=%npm_package_version% --icon=./app/favicon.ico --overwrite",
      "build": "npm run grunt-build && npm run pack && npm run installer-deb",
      "build-win": "npm run grunt-build && npm run pack-win && npm run installer",
      "build-win-linux": "npm run grunt-build && npm run pack-win-linux && npm run installer",
      "pack-all": "npm run pack && npm run pack-win",
      "build-all": "npm run grunt-build && npm run pack-all",
      "test": "./node_modules/.bin/karma start test/karma.conf.js",
      "rebuild-app": "./node_modules/.bin/electron-rebuild --version $npm_package_electronVersion --module-dir app/node_modules/ -f -p",
      "rebuild-node": "./node_modules/.bin/electron-rebuild --version $npm_package_electronVersion --module-dir ./node_modules/ -f -p",
      "rebuild": "npm run rebuild-node && npm run rebuild-app",
      "installer64": "./node_modules/.bin/grunt installer:64",
      "installer": "./installer.sh || sh ./installer.sh",
      "installer2": "./node_modules/.bin/grunt installer",
      "installer-deb": "./node_modules/.bin/electron-installer-debian --src $npm_package_electronOut/${npm_package_name}-linux-x64/ --dest release/ --arch amd64 --options.version $npm_package_version --config installer-deb.json"
    }
