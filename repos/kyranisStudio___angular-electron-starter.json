  "packageJSON": {
    "name": "build",
    "version": "1.0.0",
    "description": "A webpack starter for Angular",
    "scripts": {
      "start": "webpack-dev-server --inline --progress --port 8080",
      "install-prod": "cd production && npm install",
      "build-dev": "rimraf dist && rimraf production/app && webpack --config config/webpack.dev.build.js --progress --profile --bail  && npm run copy",
      "build-prod": "rimraf dist  && rimraf production/app && webpack --config config/webpack.prod.build.js --progress --profile --bail && npm run copy",
      "build-pack": "cd production && npm install && rimraf build-* && electron-packager . --asar --platform=all --arch=all",
      "build-dist": "rimraf dist && rimraf production/app && webpack --config config/webpack.dev.build.js --progress --profile --bail  && npm run copy && npm run build-pack",
      "run-build": "cd production && npm start",
      "test": "karma start",
      "copy": "ncp index.js dist/index.js && ncp dist production"
    },
    "licenses": [
      {
        "type": "MIT",
        "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
      }
    ],
