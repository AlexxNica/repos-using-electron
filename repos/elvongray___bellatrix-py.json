    "name": "bellatrix",
    "version": "0.1.0",
    "description": "An offline repl and markdown editor",
    "main": "main.js",
    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
      "watch": "./node_modules/.bin/webpack-dev-server",
      "bundle": "./node_modules/.bin/webpack ./app/js/app.js ./app/js/bundle.js",
      "electron-rebuild": "./node_modules/.bin/electron-rebuild",
      "build-osx": "electron-packager . Bellatrix --platform=darwin --arch=x64 --version=0.35.0 --ignore=node_modules/",
      "build-win32": "electron-packager . Bellatrix --platform=win32 --arch=ia32 --version=0.35.0 --ignore=node_modules/",
      "build-win64": "electron-packager . Bellatrix --platform=win32 --arch=x64 --version=0.35.0 --ignore=node_modules/",
      "build-linux": "electron-packager . Bellatrix --platform=linux --arch=x64 --version=0.35.0 --ignore=node_modules/"
    },
