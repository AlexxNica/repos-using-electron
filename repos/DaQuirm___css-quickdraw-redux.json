      "prod": "NODE_ENV=prod npm start",
      "start": "NODE_PATH=./app coffee index.coffee 2>>cssqderr.log",
      "test": "npm run test-backend && npm run test-frontend",
      "test-backend": "NODE_ENV=test NODE_PATH=./ mocha --compilers coffee:coffee-script/register --require ./test/chai.coffee --recursive --reporter spec",
