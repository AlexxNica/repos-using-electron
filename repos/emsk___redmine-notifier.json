      "pack:win": "./bin/pack-win.sh",
      "test": "./bin/test.sh",
      "release": "npm run prepare && npm run pack"
    },
    "dependencies": {
      "node-notifier": "^4.6.0",
      "notie": "3.9.2"
    },
    "devDependencies": {
      "chai": "^3.5.0",
      "chai-as-promised": "^5.3.0",
      "csslint": "^1.0.2",
      "electron-builder": "~5.24.1",
      "electron-prebuilt": "1.3.2",
      "eslint": "^3.2.0",
      "mocha": "^2.5.3",
      "spectron": "~3.3.0",
      "textlint": "^7.1.0",
      "textlint-rule-write-good": "^0.1.4"
    },
    "build": {
      "appId": "redmine-notifier",
      "category": "public.app-category.productivity",
      "asar": {
        "unpack": "**/app/{node_modules/node-notifier/vendor,images}/**"
      },
      "files": [
        "**/*",
        "!**/npm-debug\\.log",
        "!etc"
      ],
