    "dist:all": "npm run dist:darwin",
    "dist:darwin": "rm build/**.dmg && ./node_modules/.bin/appdmg ./config/appdmg.json build/SampleChat.dmg -v",
    "postinstall": "./node_modules/.bin/electron-rebuild && ./node_modules/.bin/bower install"
