  "packageJSON": {
    "name": "ionic2-webpack-starter",
    "version": "0.1.0",
    "description": "An Ionic 2 starter kit that uses Webpack with HMR, Karma, Protractor, Istanbul, Typescript, Typings, TSLint and Codelyzer.",
    "main": "./main_process/index.js",
    "scripts": {
      "assets": "./node_modules/gulp/bin/gulp.js prepare:assets",
      "build": "npm run build:dev",
      "build:dev": "npm run clean && npm run assets && NODE_ENV=dev webpack --config webpack.config.js  --progress --profile --colors --display-error-details --display-cached --bail",
      "build:stg": "npm run clean && npm run assets && NODE_ENV=stg webpack --config webpack.config.js  --progress --profile --colors --display-error-details --display-cached --bail",
      "build:prd": "npm run clean && npm run assets && NODE_ENV=prd webpack --config webpack.config.js  --progress --profile --colors --display-error-details --display-cached --bail",
      "clean": "rm -rf www src/assets/icons releases",
      "pkg:adr": "PLATFORM=android npm run build:prd && NODE_ENV=prd PLATFORM=android ./node_modules/gulp/bin/gulp.js package",
      "pkg:darwin": "PLATFORM=darwin npm run build:prd && NODE_ENV=prd PLATFORM=darwin ./node_modules/gulp/bin/gulp.js package",
      "pkg:linux": "PLATFORM=linux npm run build:prd && NODE_ENV=prd PLATFORM=linux ./node_modules/gulp/bin/gulp.js package",
      "pkg:window": "PLATFORM=window npm run build:prd && NODE_ENV=prd PLATFORM=window ./node_modules/gulp/bin/gulp.js package",
      "pkg:ios": "PLATFORM=ios npm run build:prd && NODE_ENV=prd PLATFORM=ios ./node_modules/gulp/bin/gulp.js package",
      "start": "npm run start:dev",
      "start:dev": "NODE_ENV=dev webpack-dev-server --config webpack.config.js --inline --progress --profile --colors --hot --display-error-details --display-cached --content-base src/",
      "start:stg": "NODE_ENV=stg webpack-dev-server --config webpack.config.js --progress --profile --colors --display-error-details --display-cached --bail",
      "start:prd": "NODE_ENV=prd webpack-dev-server --config webpack.config.js --progress --profile --colors --display-error-details --display-cached --bail",
      "start:adr": "npm run start:adr:dev",
      "start:adr:dev": "",
      "start:adr:stg": "",
      "start:adr:prd": "",
      "start:elt": "npm run start:elt:dev",
      "start:elt:dev": "NODE_ENV=dev PLATFORM=electron ./node_modules/gulp/bin/gulp.js start:elt",
      "start:elt:stg": "NODE_ENV=stg PLATFORM=electron ./node_modules/gulp/bin/gulp.js start:elt",
      "start:elt:prd": "NODE_ENV=prd PLATFORM=electron ./node_modules/gulp/bin/gulp.js start:elt",
      "start:ios": "npm run start:ios:dev",
      "start:ios:dev": "",
      "start:ios:stg": "",
      "start:ios:prd": "",
      "test": "karma start"
    },
    "author": "Cayter Goh <cayter@appist.io>",
    "license": "MIT",
    "engines": {
      "node": ">= 6.0.0",
      "npm": ">= 3.8.0"
    },
    "dependencies": {
      "@angular/common": "2.0.0-rc.1",
