  "packageJSON": {
    "name": "gwatch",
    "productName": "GWatch",
    "description": "Graphical interface for GWatchD",
    "version": "0.1.0",
    "homepage": "http://gorzechowski.github.io/gwatch",
    "author": {
      "name": "Gracjan Orzechowski",
      "email": "kampo2@o2.pl"
    },
