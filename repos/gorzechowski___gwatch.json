  "packageJSON": {
    "name": "gwatch",
    "productName": "GWatch",
    "description": "Graphical interface for GWatchD",
    "version": "0.2.0",
    "homepage": "http://gorzechowski.github.io/gwatch",
    "license": "ISC",
