      "lodash": "^4.16.0",
      "mobx": "^2.5.1",
      "mobx-react": "^3.5.5",
      "react": "^15.3.1",
      "react-desktop": "^0.2.13",
      "react-dom": "^15.3.1",
      "react-dropdown": "^1.1.0",
      "react-icons": "^2.2.1",
      "react-lazyload": "^2.1.5",
      "shell-escape-tag": "^1.0.2"
