  "packageJSON": {
    "name": "electron-tray-player",
    "productName": "electron-tray-player",
    "version": "1.0.0",
    "description": "Simple MP3/Radio player - system tray application (Electron + Vue.js)",
    "main": "index.js",
    "scripts": {
      "test": "standard",
      "start": "gulp serve",
      "build": "electron-packager . TrayPlayer --platform=darwin --arch=x64 --version=1.2.0 --app-version=1.0.0 --overwrite --asar=false --icon='./images/App.icns' --ignore='node_modules/electron-(packager|prebuild)'",
      "build:all": "electron-packager . TrayPlayer --platform=all --arch=x64 --version=1.2.0 --app-version=1.0.0 --overwrite --asar=false --ignore='node_modules/electron-(packager|prebuild)'"
    },
    "repository": "https://github.com/vlewin/electron-tray-player",
    "license": "MIT",
    "dependencies": {
      "glob": "^7.0.3",
      "jsdom": "latest",
      "lastfmapi": "^0.1.1",
      "menubar": "latest",
      "store": "latest",
      "vue": "latest",
      "id3js": "^1.1.3"
    },
    "devDependencies": {
      "electron-connect": "latest",
      "electron-packager": "latest",
      "electron-prebuilt": "latest",
      "electron-rpc": "latest",
      "eslint-config-airbnb": "^9.0.1",
      "eslint-plugin-html": "^1.5.1",
      "eslint-plugin-import": "^1.8.1",
      "eslint-plugin-jsx-a11y": "^1.2.2",
      "gulp": "latest",
      "standard": "latest"
    },
    "standard": {
      "ignore": [
        "TrayPlayer.app/**",
        "TrayPlayer-darwin-x64.app/**"
      ]
    },
    "other": {
      "author": "vlewin"
    }
  },
