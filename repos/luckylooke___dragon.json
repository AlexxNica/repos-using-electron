  "packageJSON": {
    "name": "dragon",
    "version": "0.0.1",
    "description": "Drag and drop SOLID",
    "main": "dragon.js",
    "scripts": {
      "build": "npm run scripts && npm run styles",
      "deploy": "npm run build && npm run deployment && npm run sync",
      "deploy-plugins": "npm run deploy-react ; npm run deploy-angular ; cd ../dragon",
      "deploy-react": "cd ../react-dragon ; git pull ; npm i -S dragon@* ; git commit package.json -m 'bumped dragon version'; npm run deploy",
      "deploy-angular": "cd ../angular-dragon ; git pull ; npm i -S dragon@* ; git commit package.json -m 'bumped dragon version'; npm run deploy",
      "deployment": "git add dist && npm version ${BUMP:-\"patch\"} --no-git-tag-version && git add package.json && git commit -am \"Autogenerated pre-deployment commit\" && bower version ${BUMP:-\"patch\"} && git reset HEAD~2 && git add . && git commit -am \"Release $(cat package.json | jq -r .version)\" && git push --tags && npm publish && git push",
      "scripts": "jshint . && browserify -s dragon -do dist/dragon.js dragon.js && uglifyjs -m -c -o dist/dragon.min.js dist/dragon.js",
      "dev": "watchify -dvo example/example.min.js example/example.js & watchify -dvs dragon -o dist/dragon.js dragon.js & stylus -w dragon.styl --import node_modules/nib -o dist",
      "styles": "stylus dragon.styl --import node_modules/nib -o dist && cleancss dist/dragon.css -o dist/dragon.min.css",
      "sync": "git checkout gh-pages ; git merge master ; git push ; git checkout master",
      "lint": "jshint . --reporter node_modules/jshint-stylish/index.js",
      "test": "npm run lint && browserify test/*.js | tape-run",
      "test-watch": "hihat test/*.js -p tap-dev-tool"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/bevacqua/dragula.git"
    },
    "author": "Ctibor Laky <luckylooke@gmail.com> (github.com/luckylooke)",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/luckylooke/dragon/issues"
    },
    "homepage": "https://github.com/luckylooke/dragon",
    "dependencies": {
      "contra": "1.9.4",
      "crossvent": "1.5.4",
      "diamond": "0.0.2",
      "middle.js": "^1.0.0"
    },
    "devDependencies": {
      "bower": "1.5.2",
      "browserify": "11.0.0",
      "clean-css": "3.3.6",
      "electron-prebuilt": "0.30.1",
      "hihat": "1.2.1",
      "jshint": "2.8.0",
      "jshint-stylish": "2.0.1",
      "nib": "1.1.0",
      "phantomjs": "2.1.3",
      "stylus": "0.52.0",
      "tap-dev-tool": "1.3.0",
      "tape": "4.0.1",
      "tape-run": "^2.1.3",
      "uglify-js": "2.4.24",
      "watchify": "3.3.0"
    }
  },
  "packageStatus": 200
