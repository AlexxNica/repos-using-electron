      "electron-prebuilt": "^1.2.6",
      "electron-windows-store": "^0.3.0",
      "electron-winstaller": "^2.2.0",
      "mocha": "^2.3.4",
      "request": "^2.70.0",
      "rimraf": "^2.5.2",
      "signcode": "^0.4.0",
      "spectron": "~3.2.6",
      "standard": "^6.0.8"
    },
