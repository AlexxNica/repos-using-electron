      "expect.js": "^0.3.1",
      "istanbul": "^1.1.0-alpha.1",
      "keen-tracking": "^1.0.4",
      "mocha": "^3.1.2",
      "node-sass": "^3.10.1",
      "nodemon": "^1.11.0",
      "opbeat": "^3.19.2",
      "sass-loader": "^4.0.2",
      "snyk": "^1.19.1",
      "style-loader": "^0.13.1",
      "tap": "^8.0.0",
      "webpack": "^1.13.3",
      "xo": "^0.17.0"
    },
