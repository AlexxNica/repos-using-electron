  "owner": "werk85",
  "scripts": {
    "build:browser": "webpack --config webpack.config.js",
    "build:electron:renderer": "webpack --config webpack.config.js --platform electron",
    "build:electron": "rimraf www/** && NODE_ENV=development npm run build:electron:main && NODE_ENV=development npm run build:electron:renderer",
    "build:electron:main": "webpack --config webpack.config.electron.js",
    "build:electron:production": "rimraf www/** && NODE_ENV=production npm run electron:main:build && NODE_ENV=development npm run electron:renderer:build",
    "dist:electron": "npm run build:electron && npm run electron:package:win && npm run electron:installer:win",
    "dist:electron:production": "npm run build:electron:production && npm run electron:package:win && npm run electron:installer:win:production",
    "start": "webpack-dev-server --inline --hot --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron:main": "TS_NODE=true electron -r ts-node/register ./src/electron/main/index.ts",
    "electron:renderer:watch": "webpack-dev-server --inline --progess --hot --config webpack.config.js --platform electron",
    "electron:run": "npm run build:electron && electron ./www",
    "electron:run:production": "npm run build:electron:production && electron ./www",
    "electron:package:win": "rimraf .tmp/electron/packages/** && node ./scripts/electron/packager",
    "electron:installer:win": "NODE_ENV=development node ./scripts/electron/winstaller",
    "electron:installer:win:production": "NODE_ENV=production node ./scripts/electron/winstaller",
    "postinstall": "typings -- install"
  },
