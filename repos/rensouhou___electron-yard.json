    "name": "electron-test",
    "version": "0.4.0",
    "description": "Dockyard-as-an-App",
    "main": "main.js",
    "scripts": {
      "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --recursive",
      "test-watch": "mocha --recursive --compilers js:babel-register --watch",
      "start": "cross-env NODE_ENV=products electron ./",
      "start-dev": "cross-env ELECTRON_ENABLE_LOGGING=true NODE_ENV=development electron ./",
      "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development.js",
      "webpack-dev": "webpack --config webpack.config.development.js --progress --colors",
      "webpack-dev-watch": "webpack --config webpack.config.development.js --progress --watch --colors",
      "test": "mocha --recursive --compilers js:babel-register",
      "lint": "eslint app *.js",
      "hot-server": "node server.js",
      "dev": "npm run webpack-dev; concurrently --kill-others \"npm run webpack-dev-watch\" \"npm run start-dev\"",
      "dev-hot": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
      "package": "cross-env NODE_ENV=production node -r babel-register package.js --name Dockyard",
      "package-all": "npm run package -- --all"
    },
