  "packageJSON": {
    "name": "site-switcher",
    "version": "0.2.0",
    "description": "Web and Electron apps to display multiple websites (one after another).",
    "main": "src/main.jsx",
    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "dev:web": "webpack-dev-server -d --hot --inline --progress --colors",
      "build:web": "webpack --progress --colors",
      "prepare:dist": "rm -rf electron-app/dist && mkdir electron-app/dist",
      "app:dev": "npm run prepare:dist && webpack --watch --progress --colors --config webpack.electron.js",
      "app:run": "node_modules/.bin/electron electron-app",
      "build:app:linux": "npm run prepare:dist && webpack --progress --colors --config webpack.electron.js && cd electron-app && build --linux",
      "build:app:arm": "npm run prepare:dist && webpack --progress --colors --config webpack.electron.js && cd electron-app && build --armv7l"
    },
    "author": "https://github.com/jacekk",
