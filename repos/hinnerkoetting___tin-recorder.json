    "name": "tunein-recorder",
    "version": "1.0.0",
    "description": "Record tune-in radios",
    "main": "main.js",
    "scripts": {
      "clean": "rimraf target",
      "start": "electron app/main.js",
      "build": "cd app && npm run build",
      "release-zip-packager": "cd app && npm run release && cd .. && electron-zip-packager app --all --out=target/",
