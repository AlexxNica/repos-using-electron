    },
    "homepage": "https://github.com/bjarneo/HNU#readme",
    "devDependencies": {
      "electron-packager": "7.0.0",
      "electron-prebuilt": "0.37.8",
      "eslint": "2.9.0",
      "eslint-config-vgno": "6.0.0"
    },
    "dependencies": {
      "electron-json-storage": "^2.0.0"
    }
  },
