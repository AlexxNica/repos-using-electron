    "main": "main.js",
    "devDependencies": {
      "babel-plugin-external-helpers": "^6.8.0",
      "babel-preset-es2015": "^6.14.0",
      "electron": "^1.3.5",
      "electron-packager": "^8.0.0",
      "rollup": "^0.35.7",
      "rollup-plugin-babel": "^2.6.1",
      "rollup-plugin-uglify": "^1.0.1",
      "rollup-watch": "^2.5.0"
    },
    "scripts": {
      "dev": "cp tools/index.dev.html app/index.html && rollup -cwm",
      "release": "rm -f app/sculptgl.js.map && cp tools/index.release.html app/index.html && rollup -c --environment release",
      "buildElectron": "npm run release && rm -rf standalone/app/ && cp -R app/ standalone/ && cp package.json standalone",
      "standalone": "npm run buildElectron && cd standalone && node buildStandalone.js"
    },
