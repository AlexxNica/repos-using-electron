  "packageJSON": {
    "name": "angular-webpack-npm-seed",
    "version": "1.0.0",
    "description": "Playground to test a local development environment focused around webpack and npm scripts",
    "author": "Andrew Hamilton <andrew.hamilton.dev@gmail.com>",
    "license": "MIT",
    "main": "./src/app/app.js",
    "scripts": {
      "postinstall": "webdriver-manager update --standalone",
      "start": "opener http://localhost:8000 & node ./webpack-server.js",
      "json-server": "json-server --port 3000 --routes ./src/db/routes.json --watch ./src/db/db.json",
      "prerelease": "npm test && npm run linters",
      "release": "webpack --config ./webpack-release.config.js -p --bail",
      "postrelease": "npm run copy-assets",
      "copy-assets": "cp ./src/index.html ./dist/index.html && cp -r ./src/assets ./dist/assets",
      "linters": "npm run eslint && npm run sasslint",
      "sasslint": "sass-lint --verbose",
      "eslint": "eslint ./src/app",
      "docs": "npm run esdoc && npm run sassdoc",
      "esdoc": "esdoc -c ./esdoc.config.json",
      "sassdoc": "sassdoc ./src -d src/docs/sassdoc",
      "test": "karma start karma.config.js --single-run",
      "tdd": "karma start karma.config.js --auto-watch --no-single-run",
      "protractor": "webdriver-manager start & protractor ./protractor.config.js",
      "electron:build": "npm run electron:prep && npm run electron:dev",
      "electron:dev": "electron ./electron",
      "electron:prep": "rsync -r --exclude 'index.html' ./dist/* ./electron"
    },
    "config": {
      "ghooks": {
        "pre-commit": "npm run eslint && npm run sasslint && npm run test"
      }
    },
    "devDependencies": {
      "angular": "^1.4.5",
