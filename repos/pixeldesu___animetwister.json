  "name": "animetwister",
  "version": "1.0.0",
  "description": "AnimeTwist electron wrapper",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build:macos && npm run build:linux && npm run build:windows",
    "build:macos": "electron-packager . --overwrite --asar --out=dist --ignore='^node_modules$' --prune --platform=darwin --arch=x64 --app-bundle-id=com.pixeldesu.animetwister",
    "build:linux": "electron-packager . --overwrite --out=dist --ignore='^node_modules$' --prune --platform=linux --arch=x64 --app-bundle-id=com.pixeldesu.animetwister",
    "build:windows": "electron-packager . --overwrite --asar --out=dist --ignore='^node_modules$' --prune --platform=win32 --arch=ia32 --version-string.ProductName=$npm_package_productName"
  },
  "repository": {
