  "packageJSON": {
    "name": "TradeJS",
    "version": "0.0.10",
    "description": "Javascript Trading Client",
    "main": "index.js",
    "license": "Apache License 2.0",
    "repository": "https://github.com/DutchKevv/TradeJS",
    "keywords": [
      "TradeJS",
