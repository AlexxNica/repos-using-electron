      "build:linux": "electron-packager . --overwrite --out=dist --ignore='^asset$' --platform=linux --arch=x64 --app-bundle-id=net.1000ch.whale && cd dist/Whale-linux-x64/ && zip -ryq9 ../Whale-linux-${npm_package_version}.zip *",
