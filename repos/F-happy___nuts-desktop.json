      "gulp-plumber": "^1.1.0",
      "gulp-replace-pro": "^1.0.1",
      "gulp-sass": "^2.3.2",
      "node-json-file": "^1.0.0",
      "nuts-scss": "^1.2.0",
      "through2": "^2.0.1",
      "node-fetch": "^1.6.3",
      "vue": "^2.0.0",
      "webpack-stream": "^3.2.0"
    }
