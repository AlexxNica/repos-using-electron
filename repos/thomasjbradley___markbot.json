  "packageJSON": {
    "name": "Markbot",
    "version": "3.6.0",
    "license": "GPL",
    "description": "An automated marking application for code projects in the Algonquin College Graphic Design program.",
    "author": "Thomas J Bradley <hey@thomasjbradley.ca>",
    "homepage": "https://github.com/thomasjbradley/markbot",
    "bugs": "https://github.com/thomasjbradley/markbot/issues",
    "private": true,
    "main": "markbot.js",
    "dependencies": {
      "cheerio": "^0.19.0",
      "css": "^2.2.1",
      "entities": "^1.1.1",
      "eslint": "^2.4.0",
      "git-commits": "^1.1.1",
      "git-get-status": "^1.0.2",
      "htmlcs": "^0.1.12",
      "jimp": "^0.2.21",
      "js-beautify": "^1.6.2",
      "js-yaml": "^3.5.4",
      "merge-objects": "^1.0.5",
      "mkdirp": "^0.5.1",
      "node-dir": "^0.1.11",
      "stylelint": "^4.5.1",
      "xml2js": "^0.4.16"
    },
    "devDependencies": {
      "electron-builder": "^2.10.1",
      "electron-packager": "^5.2.1",
      "electron-prebuilt": "^0.37.2",
      "rimraf": "^2.5.2"
    },
    "scripts": {
      "start": "electron markbot.js",
      "clean-osx": "rimraf ./dist/osx",
      "clean-win": "rimraf ./dist/win",
      "clean": "rimraf ./dist",
      "pack-osx": "npm run clean-osx && electron-packager ./ --overwrite --app-version=3.6.0 --build-version=3.6.0 \"Markbot\" --out=dist/osx --platform=darwin --arch=x64 --asar --asar-unpack-dir=vendor --version=0.37.2 --ignore='(dist/osx|dist/win)' --icon=images/icon.icns --app-copyright='© Thomas J Bradley' --app-bundle-id=ca.thomasjbradley.markbot --app-category-type=public.app-category.developer-tools",
      "pack-win": "npm run clean-win && electron-packager ./ --overwrite --app-version=3.6.0 --build-version=3.6.0 \"Markbot\" --out=dist/win --platform=win32 --arch=ia32 --asar --asar-unpack-dir=vendor --version=0.37.2 --ignore='(dist/osx|dist/win)' --icon=images/icon.ico --app-copyright='© Thomas J Bradley' --version-string={ProductName:'Markbot',LegalCopyright:'© Thomas J Bradley'}",
      "pack": "npm run clean && npm run pack-osx && npm run pack-win",
      "build-osx": "npm run pack-osx && electron-builder \"dist/osx/Markbot-darwin-x64/Markbot.app\" --platform=osx --out=\"dist/osx\" --config=builder.json",
      "build-win": "npm run pack-win && electron-builder \"dist/win/Markbot-win32-ia32\" --platform=win --out=\"dist/win\" --config=builder.json",
      "build": "npm run build-osx && npm run build-win",
      "hash-passcode": "node ./scripts/hash-passcode.js",
      "version": "node ./scripts/version.js && git add package.json builder.json"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/thomasjbradley/markbot.git"
    }
  },
  "packageStatus": 200
