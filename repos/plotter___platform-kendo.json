      "gulp": "^3.8.10",
      "gulp-cached": "^1.1.0",
      "gulp-changed": "^1.3.2",
      "gulp-less": "^3.1.0",
      "gulp-rename": "^1.2.2",
      "gulp-requirejs-optimize": "^1.2.0",
      "gulp-typescript": "*",
      "merge2": "*",
      "path": "^0.12.7",
      "run-sequence": "^1.0.2",
      "text": "github:requirejs/text",
      "tslint": "^3.11.0",
      "typescript": ">=1.9.0-dev || ^2.0.0"
    }
