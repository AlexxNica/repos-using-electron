  "packageJSON": {
    "name": "ELaunch",
    "version": "0.0.3",
    "main": "index.js",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/zaaack/ELaunch.git"
    },
    "keywords": [
      "launcher",
      "electron"
    ],
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/zaaack/ELaunch/issues"
    },
