    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "mocha",
    "electron",
    "test",
    "tests"
  ],
  "dependencies": {
    "cross-spawn": "^4.0.0",
    "electron-mocha": "^2.2.0",
    "object-to-spawn-args": "^1.1.0",
    "gulp-util": "^3.0.7",
    "requireg": "^0.1.5",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "electron-prebuilt": "^1.2.2",
    "mocha": "^2.5.3",
    "test-console": "^1.0.0",
    "xo": "^0.15.1"
  },
  "xo": {
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-var": 2,
      "prefer-const": [
        2,
        {
          "destructuring": "all"
        }
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "babel/object-curly-spacing": 0,
      "space-before-function-paren": [
        2,
        "never"
      ],
      "generator-star-spacing": [
        2,
        "after"
      ]
    },
    "envs": [
      "node",
      "mocha"
    ]
  }
