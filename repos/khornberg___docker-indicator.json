    "name": "docker-indicator",
    "version": "0.2.1",
    "description": "Shows docker status at a glance",
    "dependencies": {
      "docker-events": "0.0.2",
      "dockerode": "^2.2.3",
      "file-exists": "^1.0.0",
      "menubar": "4.1.0",
