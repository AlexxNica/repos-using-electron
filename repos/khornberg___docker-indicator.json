    },
    "devDependencies": {
      "electron-builder": "^2.1.1",
      "electron-packager": "^5.1.1",
      "electron-prebuilt": "0.36.12"
    },
    "main": "main.js",
    "scripts": {
      "pack": "node_modules/.bin/electron-packager . docker-indicator --out=dist --platform=darwin --arch=x64 --version=0.36.12 --ignore=\"node_modules/(electron-packager|electron-prebuilt|electron-builder)\" --icon=images/icons/whale.icns --build-version=0.1.0",
      "build": "npm run pack && node_modules/.bin/electron-builder \"dist/docker-indicator-darwin-x64/docker-indicator.app\" --platform=osx --out=\"dist\" --config=packager.json",
      "start": "electron ."
