  "packageJSON": {
    "name": "DotMD",
    "version": "1.0.0",
    "main": "app/main/index.js",
    "scripts": {
      "dev": "ELECTRON_ENV=development electron ./",
      "test": "mocha --compilers js:babel-register --recursive",
      "package": "electron-packager ./ 'DotMD' --out=./build/1.0.0/ --platform=darwin --icon=./icon.icns --asar=true --overwrite=true --prune=true --ignore='src|test|build|gulpfile.js|config/default.json|config/development.json'",
      "cleanup": "rm -rf app",
      "mas-package": "npm run cleanup && gulp compile && npm run-script package && cd ./mas-build && bash ./build.sh"
    },
    "author": "taqun",
