      "chai": "^3.4.1",
      "chai-as-promised": "^5.1.0",
      "devtron": "^1.0.0",
      "electron-installer-dmg": "^0.1.1",
      "electron-packager": "^7.0.1",
      "electron-prebuilt": "^1.0.2",
      "electron-winstaller": "^2.2.0",
      "mocha": "^2.3.4",
      "request": "^2.70.0",
      "rimraf": "^2.5.2",
      "signcode": "^0.4.0",
      "spectron": "~3.0.0",
      "standard": "^7.0.1"
    },
    "private": true,
    "standard": {
      "env": {
        "mocha": true
      }
    }
