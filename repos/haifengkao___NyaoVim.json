  "packageJSON": {
    "name": "nyaovim",
    "productName": "NyaoVim",
    "version": "0.0.20",
    "description": "Web-enhanced Extensible Neovim Frontend",
    "main": "main/main.js",
    "bin": "./bin/cli.js",
    "scripts": {
      "dep": "npm install && bower install && typings install",
      "build-main": "tsc --pretty -p main",
      "build-renderer": "tsc --pretty -p renderer",
      "build": "npm run build-main && npm run build-renderer",
      "pack": "npm run build && build --dir",
      "dist": "npm run build && build",
      "lint": "tslint $(git ls-files | grep -E \".ts$\")",
      "app": "electron $(npm prefix)",
      "debug": "NODE_ENV=debug electron $(npm prefix)",
      "start": "npm run dep && npm run build && npm run app",
      "watch": "guard --watchdir main renderer typings",
      "clean": "rm -rf build typings bower_components node_modules"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/rhysd/NyaoVim.git"
    },
    "build": {
      "asar": false,
      "appId": "org.vim.NyaoVim",
      "app-category-type": "public.app-category.utilities",
      "win": {
        "remoteReleases": "https://github.com/rhysd/NyaoVim",
        "icon": "./resources/icon/nyaovim-logo.ico"
      },
      "mac": {
        "icon": "./resources/icon/nyaovim-logo.icns"
      },
      "dmg": {
        "icon": "./resources/icon/nyaovim-logo.icns"
      },
      "linux": {
        "target": [
          "deb",
          "AppImage"
        ]
      }
    },
    "keywords": [
      "editor",
      "extensible",
      "WebComponents",
      "Neovim",
      "nvim",
      "Electron",
      "Polymer",
      "cat",
      "meow"
    ],
    "author": "rhysd <lin90162@yahoo.co.jp>",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/rhysd/NyaoVim/issues"
    },
    "homepage": "https://github.com/rhysd/NyaoVim#readme",
    "dependencies": {
      "about-window": "^1.3.1",
      "deep-extend": "^0.4.1",
      "electron-window-state": "^3.0.3",
      "mkdirp": "^0.5.1",
      "neovim-component": "^0.7.2",
      "promised-neovim-client": "^1.1.1"
    },
    "devDependencies": {
      "bower": "^1.7.1",
      "tslint": "^3.14.0",
      "electron-builder": "^5.22.2",
      "electron-prebuilt": "^1.3.2",
      "typescript": "^2.0.0",
      "typings": "^1.3.2"
    }
  },
  "packageStatus": 200
}