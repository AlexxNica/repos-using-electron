    "grunt-lintspaces": "^0.6.0",
    "mocha": "^2.2.5",
    "wd": "^0.3.12",
    "webdriver-manager": "^7.0.1"
  },
  "single-instance": true,
  "window": {
    "title": "plaidchat",
    "icon": "./app/images/app-256.png",
    "width": 650,
    "height": 650,
    "min_width": 650,
    "min_height": 650,
    "toolbar": false
  },
  "webkit": {
