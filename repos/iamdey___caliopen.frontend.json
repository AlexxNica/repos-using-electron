  "packageJSON": {
    "name": "caliopen.frontend",
    "version": "0.0.1",
    "description": "CaliOpen UI is the user interface for CaliOpen.",
    "homepage": "https://caliopen.org/",
    "private": true,
    "main": "index.js",
    "scripts": {
      "start": "webpack-dashboard -- npm-run-all --parallel --race start:dev:server start:dev:assets",
      "start:dev:server": "NODE_ENV=development PORT=3001 kotatsu start -c webpack/webpack.config.server.js",
      "start:dev:assets": "NODE_ENV=development kotatsu serve -c webpack/webpack.config.browser.js -p 3000 --proxy / http://localhost:3001",
      "start:prod": "NODE_ENV=production ./bin/server",
      "start:desktop": "npm-run-all --parallel --race build:desktop:dev start:desktop:electron",
      "start:desktop:electron": "NODE_ENV=development electron ./electron/",
      "start:smartphone": "npm-run-all --parallel --race build:smartphone:dev start:smartphone:android",
      "start:smartphone:android": "cd cordova && nodemon --watch www --exec 'cordova emulate android --debug'",
      "build:desktop:dev": "NODE_ENV=development webpack-dashboard -- webpack --watch --config webpack/webpack.config.electron.js",
      "build:smartphone:dev": "NODE_ENV=development webpack-dashboard -- webpack --watch --config webpack/webpack.config.cordova.js",
      "release": "NODE_ENV=production webpack-dashboard -- kotatsu build server --config webpack/webpack.config.server.js",
      "cordova": "cd cordova && cordova",
      "test": "npm-run-all lint test:unit",
      "test:unit": "jest",
      "test:unit:watch": "jest --watch",
      "lint": "npm-run-all lint:js lint:sass",
      "lint:js": "eslint src/",
      "lint:sass": "sass-lint 'src/**/*.scss'"
    },
    "license": "MIT",
    "dependencies": {
      "babel-core": "^6.17.0",
