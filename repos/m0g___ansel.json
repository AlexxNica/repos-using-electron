    "scripts": {
      "postinstall": "./node_modules/.bin/electron-rebuild && npm run rebuild-sqlite3 && npm run rebuild-exiv2",
      "rebuild-exiv2": "cd node_modules/exiv2 && node-gyp configure --module_name=exiv2 --module_path=../lib/binding/node-v49-linux-x64 && node-gyp rebuild --target=0.37.3 --arch=x64 --target_platform=linux --dist-url=https://atom.io/download/atom-shell --module_name=exiv2 --module_path=../lib/binding/node-v49-linux-x64",
      "rebuild-sqlite3": "cd node_modules/sqlite3 && npm run prepublish && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v49-linux-x64 && node-gyp rebuild --target=0.37.3 --arch=x64 --target_platform=linux --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/node-v49-linux-x64"
    },
