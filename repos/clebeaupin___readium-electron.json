    "babel": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-inline-environment-variables"
      ]
    },
    "scripts": {
      "build-renderer": "./node_modules/.bin/webpack --config webpack.config.renderer.js",
      "build-main": "./node_modules/.bin/webpack --config webpack.config.main.js",
      "build-readium": "./node_modules/.bin/webpack --config webpack.config.readium.js",
      "build-zip": "./node_modules/.bin/webpack --config webpack.config.zip.js",
      "build-all": "npm run build-readium && npm run build-main && npm run build-renderer",
      "start": "./node_modules/.bin/electron -r babel-register .",
      "start-dev-renderer": "cross-env ENVIRONMENT=DEV ./node_modules/.bin/webpack-dev-server --hot --inline --config webpack.config.renderer.js",
      "start-dev-main": "cross-env APP_ENTRY_RELATIVE_URL='/../renderer/index.html' ENVIRONMENT=DEV ./node_modules/.bin/electron -r babel-register .",
      "start-dev-all": "concurrently --kill-others \"npm run start-dev-renderer\" \"npm run start-dev-main\"",
      "pack": "node -r babel-register pack.js",
      "dist": "node -r babel-register dist.js"
    }
