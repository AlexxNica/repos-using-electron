  "packageJSON": {
    "name": "black-screen",
    "productName": "Black Screen",
    "version": "0.0.1",
    "description": "A terminal emulator for the 21st century.",
    "main": "compiled/src/main/Main.js",
    "authors": [
      "Volodymyr Shatsky <shockone89@gmail.com>",
      "Konstantin Azizov <azizovkostya97@gmail.com>",
