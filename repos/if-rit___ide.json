      "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./config/webpack/production.js --progress --profile --colors",
      "build": "npm run build-main && npm run build-renderer",
      "start": "cross-env NODE_ENV=production electron ./app",
      "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/main",
      "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
