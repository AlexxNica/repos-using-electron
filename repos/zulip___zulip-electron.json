  "packageJSON": {
    "name": "Zulip-Desktop",
    "productName": "Zulip",
    "version": "0.4.2",
    "description": "Zulip Desktop App",
    "license": "MIT",
    "repository": "https://github.com/zulip/zulip-electron",
    "main": "app/main/index.js",
    "scripts": {
      "start": "electron .",
      "test": "gulp test && xo",
      "dev": "gulp dev",
      "build:osx": "electron-packager . --out=dist --app-version=$npm_package_version --prune --asar --icon=app/resources/Icon.icns --overwrite --platform=darwin --arch=x64",
      "build": "electron-packager . --out=dist --app-version=$npm_package_version --prune --asar --overwrite --all",
      "travis": "cd ./scripts && ./travis-build-test.sh"
    },
    "keywords": [
      "Zulip",
      "Group Chat app",
      "electron-app",
      "electron"
    ],
    "dependencies": {
      "configstore": "^2.0.0",
      "dialogs": "1.1.14",
      "electron-context-menu": "0.4.0",
      "electron-debug": "^1.0.0",
      "electron-dl": "^0.2.0",
      "electron-localshortcut": "^0.6.1",
      "node-json-db": "^0.7.2",
      "request": "^2.74.0",
      "simple-spellchecker": "0.9.0",
      "wurl": "^2.1.0"
    },
    "devDependencies": {
      "devtron": "^1.1.0",
      "electron": "1.4.3",
      "assert": "^1.4.1",
      "gulp-mocha": "^3.0.1",
      "electron-connect": "^0.4.6",
      "electron-packager": "^7.0.0",
      "gulp": "^3.9.1",
      "spectron": "^3.3.0",
      "xo": "*"
    },
    "xo": {
      "esnext": true,
      "overrides": [
        {
          "files": "app/main/*.js",
          "rules": {
            "max-lines": [
              "warn",
              350
            ],
            "no-warning-comments": 0,
            "no-else-return": 0
          }
        }
      ],
      "ignore": [
        "tests/*.js"
      ],
      "envs": [
        "node",
        "browser",
        "mocha"
      ]
    },
    "other": {
      "bugs": {
        "url": "https://github.com/zulip/zulip-electron/issues"
      }
    }
