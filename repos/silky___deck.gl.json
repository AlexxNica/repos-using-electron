      "zip_url": "https://github.com/uber/deck.gl/archive/master.zip",
      "api_url": "https://api.github.com/repos/uber/deck.gl"
    },
    "main": "index.js",
    "files": [
      "dist",
      "shaderlib",
      "src"
    ],
    "standard": {
      "ignore": [
        "**/build/**",
        "**/dist/**",
        "**/examples/**",
        "**/Documents/**"
      ]
    },
    "dependencies": {
      "autobind-decorator": "^1.3.3",
      "babel-core": "^6.7.7",
      "babel-polyfill": "^6.1.19",
      "earcut": "^2.0.6",
