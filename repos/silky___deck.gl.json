      "lodash.where": "^3.1.0",
      "save-pixels": "^2.3.0",
      "viewport-mercator-project": "^2.0.1",
      "webgl-obj-loader": "^0.1.1"
    },
    "devDependencies": {
      "babel-cli": "^6.3.15",
      "babel-eslint": "^4.1.6",
      "babel-plugin-glslify": "^1.0.0",
