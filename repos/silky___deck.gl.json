      "faucet": "0.0.1",
      "gl": "^4.0.1",
      "glslify": "^5.0.0",
      "immutable": "^3.7.5",
      "jsdom": "^9.2.1",
      "luma.gl": "^2.4.1",
      "react": "^15.0.2",
      "react-addons-test-utils": "^15.0.2",
