  "packageJSON": {
    "name": "muzic",
    "version": "0.1.0",
    "description": "A minimal music player",
    "main": "main.js",
    "scripts": {
      "clean": "find dist -maxdepth 1 -type f -delete",
      "rebuild": "npm run clean && npm run build",
      "serve": "babel-node server.js",
      "start": "ENV=development electron .",
      "dev": "concurrently -k 'babel-node server.js' 'npm start'",
      "build:all": "webpack --config webpack.config.production.js && electron-packager . --all",
      "build:osx": "webpack --config webpack.config.production.js && electron-packager . muZic --platform=darwin --arch=all --overwrite"
    },
    "babel": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    },
    "repository": "https://github.com/nkprince007/muZic-electron",
    "keywords": [
      "Electron",
      "muZic",
      "music player",
      "audio",
      "player"
    ],
    "author": "nkprince007",
    "license": "MIT",
    "devDependencies": {
      "babel-cli": "^6.18.0",
