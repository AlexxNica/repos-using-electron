    },
    "homepage": "https://github.com/atom/electron-quick-start#readme",
    "devDependencies": {
      "electron-builder": "^2.5.0",
      "electron-packager": "^5.1.1",
      "electron-prebuilt": "^0.36.0"
    },
    "dependencies": {
      "node-static": "^0.7.7"
    },
    "peer-dependencies": {
      "electron": "^0.36.0"
    }
