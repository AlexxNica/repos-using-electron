  "packageJSON": {
    "name": "snap-app",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
      "start": "electron main.js $@",
      "compile": "node build/compile.js",
      "clean": "rm -rf ./binaries/osx/ ./binaries/osx/",
      "clean:osx": "rm -rf ./binaries/osx",
      "clean:win": "rm -rf ./binaries/win",
      "app": "npm run clean && npm run build:osx && npm run build:win",
      "pack:osx": "npm run clean:osx && electron-packager ./app \"Snap\" --out=binaries/osx --platform=darwin --arch=x64 --version=0.36.1 --icon=assets/osx/snap.png",
      "pack:win": "npm run clean:win && electron-packager ./app \"Snap\" --out=binaries/win --platform=win32 --arch=ia32 --version=0.36.1 --icon=assets/win/icon.ico",
      "build": "npm run pack:osx && npm run pack:win",
      "build:osx": "npm run build:osx && electron-builder \"binaries/osx/Snap.app\" --platform=osx --out=\"binaries/osx\" --config=builder.json",
      "build:win": "npm run build:win && electron-builder \"binaries/win/Snap-win32\" --platform=win --out=\"binaries/win\" --config=builder.json"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/atom/electron-quick-start.git"
    },
    "keywords": [
      "Electron",
      "quick",
      "start",
      "tutorial"
    ],
