  "packageJSON": {
    "name": "angular2-electron",
    "version": "0.0.1",
    "description": "Ringio test application",
    "scripts": {
      "rimraf": "rimraf",
      "clean": "npm cache clean && npm run rimraf -- node_modules typings build",
      "clean:build": "npm run rimraf -- build",
      "typings-install": "typings install",
      "postinstall": "npm run typings-install",
      "build": "webpack --inline --colors --progress --display-error-details --display-cached --target=electron-renderer",
      "watch": "npm run build -- --watch",
      "test": "karma start karma.conf.js",
      "electron": "electron src/main.js",
      "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000 --content-base src",
      "start": "npm run server",
      "distribute": "electron-packager . Ringio --platform=darwin --arch=x64 --out=releases/ --overwrite --icon=src/assets/images/ringio  --asar"
    },
    "author": "nnombela",
    "license": "MIT",
    "devDependencies": {
      "electron-prebuilt": "^1.2.0",
