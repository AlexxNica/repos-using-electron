    "scripts": {
      "teststart": "node_modules\\electron-prebuilt\\dist\\electron.exe bin",
      "start": "npm run start:win64",
      "start:win32": "build -w --ia32 --dir & .\\dist\\win-ia32-unpacked\\BlitzModderPC",
      "start:win64": "build -w --x64 --dir & .\\dist\\win-unpacked\\BlitzModderPC",
      "start:mac": "build -m --dir",
      "build": "gulp",
      "pack": "npm run pack:win & npm run pack:mac",
      "pack:win": "npm run build & build -w --ia32 --x64",
      "pack:mac": "npm run build & build -m"
    },
