  "packageJSON": {
    "name": "ultra-mega-git-tool",
    "productName": "UltraMegaGitTool",
    "version": "0.1.0",
    "description": "A git gui written in Elm using Electron. Cause raisins.",
    "main": "main.js",
    "scripts": {
      "test": "npm run test-elm && npm run lint && npm run test-js",
      "test-elm": "elm make tests/Elm/TestRunner.elm --output tests/elmTests.js && node tests/elmTests.js",
      "test-js": "cross-env ELECTRON_RUN_AS_NODE=1 node_modules/.bin/electron node_modules/ava/cli.js --timeout=5s --verbose --require babel-register tests/js/**.test.js",
      "test-e2e": "ava --verbose test.js",
      "lint": "standard --verbose | snazzy",
      "hot-server": "node -r babel-register util/server.js",
      "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
      "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
      "build": "npm run build-main && npm run build-renderer",
      "start": "cross-env NODE_ENV=production electron ./",
      "start-hot": "cross-env HOT=1 NODE_ENV=development node util/runHot.js",
      "package": "cross-env NODE_ENV=production node -r babel-register util/package.js",
      "package-all": "npm run package -- --all",
      "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && elm package install -y",
      "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
    },
    "bin": {
      "electron": "./node_modules/.bin/electron"
    },
    "repository": "https://github.com/kaiyote/ultra-mega-git-tool",
    "license": "MIT",
    "keywords": [
      "electron",
      "boilerplate",
      "webpack",
      "elm"
    ],
    "homepage": "https://github.com/kaiyote/ultra-mega-git-tool#readme",
