      "babel-preset-stage-0": "^6.5.0",
      "css-loader": "^0.24.0",
      "electron-packager": "^7.7.0",
      "enzyme": "^2.4.1",
      "html-webpack-plugin": "^2.22.0",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^16.0.2",
      "jest-cli": "^16.0.2",
      "jsdom": "^9.8.3",
      "node-sass": "^3.8.0",
      "react-addons-test-utils": "^15.3.1",
      "react-hot-loader": "^3.0.0-beta.6",
