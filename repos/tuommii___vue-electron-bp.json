    "electronVersion": "0.36.10",
    "scripts": {
      "test": "mocha --compilers js:babel-core/register --recursive",
      "test:watch": "npm run test --watch",
      "start": "electron .",
      "build": "NODE_ENV=production webpack -p --hide-modules",
      "deploy": "electron-packager . $npm_package_productName --out=dist --ignore='node_modules/.bin' --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion",
      "dev": "node_modules/.bin/webpack-dev-server --inline --hot --quiet"
    },
