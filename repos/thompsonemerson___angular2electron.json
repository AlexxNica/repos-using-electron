  "packageJSON": {
    "name": "angular2electron",
    "version": "0.0.1",
    "description": "A boilerplate of Angular 2 with Electron using TypeScript and Sass",
    "scripts": {
      "start": "tsc && concurrently \"tsc -w\" \"npm run sass\" \"npm run sass:w\" \"electron index.js\" ",
      "docker-build": "docker build -t ng2-quickstart .",
      "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
      "pree2e": "npm run webdriver:update",
      "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
      "lint": "tslint ./app/**/*.ts -t verbose",
      "postinstall": "typings install",
      "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
      "test-once": "tsc && karma start karma.conf.js --single-run",
      "tsc": "tsc",
      "tsc:w": "tsc -w",
      "sass": "node-sass --output-style compressed -o ./ stylesheets/styles.sass",
      "sass:w": "node-sass -w --output-style compressed -o ./ stylesheets/styles.sass",
      "typings": "typings",
      "webdriver:update": "webdriver-manager update"
    },
    "keywords": [
      "angular2",
      "electron",
      "typescript",
      "sass",
      "node"
    ],
    "licenses": "MIT",
    "dependencies": {
