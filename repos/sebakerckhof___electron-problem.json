  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build-all": "electron-packager . App --out=dist --ignore='^/dist$' --prune --asar --all --version=0.36.8",
    "build-mac": "electron-packager . App --platform=darwin --arch=x64 --version=0.36.8 --asar --out ./build/mac --prune --ignore='^/dist$' --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=node_modules/trash --ignore=.git",
    "build-linux": "electron-packager . App --platform=linux --arch=x64 --version=0.36.8 --asar --out ./build/linux --prune --ignore='^/dist$' --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=node_modules/trash --ignore=.git",
    "build-win": "electron-packager . App --platform=win32 --arch=x64 --version=0.36.8 --asar --out ./build/win --prune --ignore=node_modules/electron-prebuilt --ignore='^/dist$' --ignore=node_modules/electron-packager --ignore=node_modules/trash --ignore=.git",
    "build": "npm run build-win && npm run build-linux && npm run build-mac",
    "release-mac": "npm run build-mac && mkdir release && cd release && appdmg ../app-dmg.json 'App.dmg'"
  },
  "main": "src/index.js",
  "keywords": [
    "electron-app",
    "electron"
  ],
