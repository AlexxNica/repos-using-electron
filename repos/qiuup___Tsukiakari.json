  "packageJSON": {
    "name": "Tsukiakari",
    "version": "0.1.0",
    "description": "Multi account, multi column twitter clinet",
    "main": "/src/browser/dist/bundle.js",
    "scripts": {
      "dev": "npm run build:all && (electron src/browser/dist/bundle.js & npm run watch)",
      "flow": "flow",
      "start": "electron src/browser/dist/bundle.js",
      "build:all": "npm run build:js && npm run build:stylus",
      "watch:js": "NODE_ENV=develop watchify src/renderer/src/index.js -o src/renderer/dist/bundle.js --ignore-missing",
      "build:js": "npm run build:renderer && npm run build:main",
      "build:renderer": "NODE_ENV=production browserify --extension=js -o src/renderer/dist/bundle.js src/renderer/src/index.js --ignore-missing",
      "build:main": "NODE_ENV=production browserify --extension=js -o src/browser/dist/bundle.js src/browser/src/main.js --im --no-detect-globals --node",
      "watch": "npm run watch:js & npm run watch:stylus",
      "watch:stylus": "stylus src/renderer/stylus/ --watch  --out src/renderer/stylesheets/",
      "build:stylus": "stylus src/renderer/stylus/ --out src/renderer/stylesheets/",
      "package": "npm run build:stylus && npm run build:js && npm run minify && electron-packager ./ --platform=darwin --arch=x64 --icon=icon/tsukiakari.icns --ignore={packages,screenshots,node_modules}/* --out=packages/v0.0.1 --overwrite",
      "lint": "eslint src/renderer/src --fix",
      "minify": "uglifyjs src/renderer/dist/bundle.js -o src/renderer/dist/bundle.js",
      "test": "npm run unittest:view",
      "unittest:view": "karma start",
      "unittest:view-watch": "karma start --auto-watch --no-single-run"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/bokuweb/Tsukiakari.git"
    },
