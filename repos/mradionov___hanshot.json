    "scripts": {
      "hot-server": "webpack-dev-server --config=webpack.config.renderer.js --port=4000 --hot --inline",
      "build:main": "webpack --config webpack.config.main.js",
      "build:renderer": "webpack --config webpack.config.renderer.js",
      "build": "npm run build:main && npm run build:renderer",
      "start-hot": "HANSHOT_HOT=1 electron -r babel-register src/main.js",
      "start": "electron .",
      "package": "npm run build && node package.js",
      "lint": "eslint src/cache.js test",
      "test": "npm run test:unit && npm run test:int && npm run test:int:node",
      "test:unit": "HANSHOT_TEST=true tape -r babel-register -r ./test/unit/helpers/fake-electron.js './test/unit/**/*.test.js'",
      "test:int": "HANSHOT_TEST=true node ./test/integration/setup/run.js",
      "test:int:node": "HANSHOT_TEST=true tape -r babel-register './test/int-node/**/*.test.js'"
    }
  },
