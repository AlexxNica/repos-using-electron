      "url": "https://github.com/webofbits/electron_offline_todo/issues"
    },
    "homepage": "https://github.com/webofbits/electron_offline_todo#readme",
    "dependencies": {
      "babel": "^6.5.2",
      "babel-core": "^6.17.0",
      "cpy-cli": "^1.0.1",
      "electron": "^1.4.3",
      "http-server": "^0.9.0",
      "npm-run-all": "^3.1.0",
      "quik": "^0.7.2",
      "react-bootstrap": "^0.30.5",
      "rimraf": "^2.5.4",
      "surge": "^0.18.0"
    }
  },
