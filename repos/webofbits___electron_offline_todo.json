  "packageJSON": {
    "name": "electron_offline_todo",
    "version": "1.0.0",
    "description": "simple desktop todo list app that works offline",
    "main": "index.js",
    "scripts": {
      "start": "quik",
      "clean": "rimraf dist/ && mkdir dist/ dist/public dist/public/images dist/public/js",
      "build": "quik --html index.html --output dist/index.html --production",
      "copy": "cpy public/images/icon.png dist/public/images/ && cpy service-worker.js dist/public/js",
      "predeploy": "npm-run-all clean --parallel build copy",
      "deploy": "surge --project ./dist --domain https://electron-todo.surge.sh",
      "serve:dist": "http-server dist"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/webofbits/electron_offline_todo.git"
    },
    "author": "cristiano.codelab@gmail.com",
