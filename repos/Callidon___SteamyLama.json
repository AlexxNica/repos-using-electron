  "packageJSON": {
    "name": "SteamyLama",
    "productName": "Steamylama",
    "version": "0.8.0",
    "description": "The hotest chat app in town",
    "license": "MIT",
    "repository": "https://github.com/Callidon/SteamyLama",
    "author": {
      "name": "Thomas Minier",
      "email": "tminier01@gmail.com",
      "url": "github.com/Callidon/SteamyLama"
    },
    "main": "src/main.js",
    "electronVersion": "0.36.0",
    "scripts": {
      "test": "node node_modules/xo/cli.js app/*.js app/**/*.js src-es6/*.js src-es6/**/*.js",
      "start": "node node_modules/webpack/bin/webpack.js && node node_modules/babel-cli/bin/babel --presets es2015 src-es6/ -d src/ && electron .",
      "build": "node node_modules/webpack/bin/webpack.js -p && node node_modules/babel-cli/bin/babel --presets es2015 src-es6/ -d src/ && electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion",
      "package": "cd dist/ && zip -r Steamylama-linux-ia32.zip Steamylama-linux-ia32 && zip -r Steamylama-win32-ia32.zip Steamylama-win32-ia32 && zip -r Steamylama-darwin-x64.zip Steamylama-darwin-x64 && zip -r Steamylama-linux-x64.zip Steamylama-linux-x64 && zip -r Steamylama-win32-x64.zip Steamylama-win32-x64"
    },
    "keywords": [
      "electron-app",
      "electron",
      "twitch tv",
      "hitbox tv",
      "chat",
      "streaming"
    ],
    "dependencies": {
      "angular": "^1.4.8",
      "angular-translate": "^2.8.1",
      "angularjs-scroll-glue": "^2.0.4",
      "bootstrap": "^3.3.6",
      "electron-debug": "^0.5.0",
      "font-awesome": "^4.5.0",
      "hitbox-chat": "^0.1.5",
      "jquery": "^2.1.4",
      "ngstorage": "^0.3.10",
      "tmi.js": "0.0.24"
    },
    "devDependencies": {
      "babel": "^6.3.13",
      "babel-cli": "^6.3.17",
      "babel-preset-es2015": "^6.3.13",
      "electron-packager": "^5.0.0",
      "electron-prebuilt": "^0.36.0",
      "webpack": "^1.12.9",
      "xo": "^0.12.0"
    },
    "xo": {
      "esnext": true,
      "envs": [
        "node",
        "browser"
      ],
      "ignore": [
        "app/electron-loads.js",
        "app/bundle.js"
      ],
      "globals": [
        "angular",
        "ipcRenderer",
        "shell"
      ],
      "rules": {
        "arrow-body-style": 0,
        "new-cap": 0,
        "no-unused-vars": [
          2,
          {
            "vars": "all",
            "args": "none"
          }
        ]
      }
    }
  },
  "packageStatus": 200,
  "contributors": [
    "Callidon"
  ]
