    "directories": {
      "app": "./client/public",
      "output": "./electron-dist"
    },
    "build": {
      "appId": "com.example.electron-app"
    },
    "scripts": {
      "postinstall": "install-app-deps",
      "start": "npm run build:dev",
      "server": "lite-server -c server.config.json",
      "build:dev": "set NODE_ENV=development&& npm run copy-files && webpack --config webpack.dev.config.js --watch",
      "build:prod": "set NODE_ENV=production&& npm run copy-files && webpack --config webpack.prod.config.js",
      "copy-files": "cp client/app/package.json client/public/",
      "electron": "electron client/public/",
      "electron:server": "concurrently \"npm run server\" \"npm run electron\"",
      "electron:pack": "npm run build:prod && build --dir",
      "electron:dist": "npm run build:prod && build"
    },
