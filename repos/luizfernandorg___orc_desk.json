  "packageJSON": {
    "name": "orcamento_desktop",
    "productName": "Orcamento Domestico",
    "version": "1.0.0",
    "description": "Desktop version for personal finances",
    "homepage": "https://github.com/luizfernandorg/orc_desk",
    "main": "app/main.js",
    "scripts": {
      "start": "electron app/main.js",
      "test": "echo \"Error: no test specified\" && exit 1",
      "pack-mac": "electron-packager ./app --asar --asar-unpack=protocol-link.html --overwrite --platform=darwin --arch=x64 --icon=assets/app-icon/mac/app.icns --prune=true --out=out --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
      "pack-win": "electron-packager ./app orcamento_desktop --asar --overwrite --platform=win32 --arch=ia64 --icon=assets/app-icon/win/app.ico --prune=true --out=out --version-string.CompanyName='GitHub, Inc.' --version-string.FileDescription='Electron API Demos' --version-string.ProductName='Electron API Demos'",
      "pack-linux": "electron-packager ./app --asar --overwrite --platform=linux --arch=x64 --icon=assets/app-icon/png/64.png --prune=true --out=out",
      "package": "npm run pack-linux"
    },
    "build": {
      "appId": "com.orcamento_desktop.orc_desk.1.0.0",
      "app-category-type": "public.app-category.finance",
      "win": {
        "iconUrl": "(windows-only) https link to icon"
      }
    },
    "dependencies": {
      "nedb": "^1.8.0"
    },
    "author": "Luiz Fernando",
    "license": "GPL-3.0",
    "devDependencies": {
      "chai": "^3.5.0",
      "electron-prebuilt": "^1.2.6",
