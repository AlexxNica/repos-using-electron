      "start": "electron .",
      "start-dev": "NODE_ENV=development electron .",
      "start-hot": "HOT=1 NODE_ENV=development electron .",
      "package": "node package.js --name=GraphiQL",
      "package-all": "node package.js --name=GraphiQL --all"
