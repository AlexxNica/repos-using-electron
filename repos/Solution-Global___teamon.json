  "packageJSON": {
    "name": "teamon",
    "version": "0.1.0",
    "description": "Unified desktop app for team communication and collaboration.",
    "main": "teamon.js",
    "repository": {
      "type": "git",
      "url": "https://github.com/Solution-Global/teamon.git"
