  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf packages/v0.0.1/bloom-{darwin,win32}-x64",
    "compile": "gulp compile && npm run copy",
    "copy": "cp src/index.html dist/index.html",
    "package": "electron-packager ./ bloom --out=packages/v0.0.1 --platform=darwin,win32 --arch=x64 --version=0.34.2",
    "release": "npm run zip && npm run clean && npm run upload",
    "setup": "npm install && brew tap tcnksm/ghr && brew install ghr",
    "start": "electron .",
    "upload": "ghr -u minamimonji v0.0.1 packages/v0.0.1/",
    "watch": "gulp watch",
    "zip": "npm run zip-darwin && npm run zip-win32",
    "zip-darwin": "cd packages/v0.0.1 && zip -r bloom-darwin-x64.zip bloom-darwin-x64",
    "zip-win32": "cd packages/v0.0.1 && zip -r bloom-win32-x64.zip bloom-win32-x64"
  },
