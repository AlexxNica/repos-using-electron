    "dependencies": {
      "babel-polyfill": "6.7.4",
      "lodash": "4.11.1",
      "react": "15.1.0",
      "react-dom": "15.1.0",
      "react-addons-shallow-compare": "15.1.0",
      "sharp": "0.15.0",
      "sqlite3": "3.1.4"
    },
    "devDependencies": {
      "babel-cli": "6.7.7",
      "babel-core": "6.7.7",
      "babel-preset-es2015": "6.6.0",
      "babel-preset-react": "6.5.0",
      "babel-preset-stage-0": "6.5.0",
