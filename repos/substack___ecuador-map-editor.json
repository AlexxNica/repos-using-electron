    "dependencies": {
      "JSONStream": "^1.0.3",
      "body": "^5.1.0",
      "concat-stream": "^1.5.1",
      "ecstatic": "^1.4.0",
      "electron-debug": "^0.5.2",
      "end-of-stream": "^1.1.0",
      "hyperlog": "^4.8.1",
      "insert-css": "^0.2.0",
      "level": "^1.4.0",
      "levelup": "^1.3.1",
      "minimist": "^1.2.0",
      "once": "^1.3.3",
      "osm-p2p": "^1.2.2",
      "osm-p2p-server": "^1.5.0",
      "pump": "^1.0.1",
      "randombytes": "^2.0.3",
      "shpjs": "^3.3.1",
      "split2": "^2.0.1",
      "sqldown": "^2.1.0",
