{
  "name": "Inbox",
  "version": "0.1.3",
  "description": "A wrapper for Google Inbox",
  "scripts": {
    "start": "electron src/app.js",
    "build": "gulp build",
    "version": "gulp sync-package-files && git add src/package.json && gulp build",
    "postversion": "git push",
    "compress": "cd dist && ditto -c -k --sequesterRsrc --keepParent ./$npm_package_name-darwin-x64 ./$npm_package_name-$npm_package_version.zip && cd ..",
    "create-autoupdater-file": "gulp create-autoupdater-file && git add auto_updater.json && git commit -m 'Updates auto updater file' && git push",
    "release": "npm run compress && npm run create-autoupdater-file && gulp release"
  },
  "repository": "github:martinlissmyr/deskbox",
  "keywords": [
    "desktop",
    "inbox",
    "mail",
    "wrapper"
  ],
  "author": "Martin Lissmyr",
