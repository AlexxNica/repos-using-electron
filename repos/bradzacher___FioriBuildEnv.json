      "extend": "^3.0.0",
      "gulp": "^3.9.1",
      "gulp-babel": "^6.1.2",
      "gulp-concat": "^2.6.0",
      "gulp-cssmin": "^0.1.7",
      "gulp-eslint": "^3.0.1",
      "gulp-html-replace": "^1.5.5",
      "gulp-htmlmin": "^1.3.0",
      "gulp-rename": "^1.2.2",
      "gulp-sass": "^2.2.0",
      "gulp-scss-lint": "^0.3.9",
      "gulp-size": "^2.1.0",
      "gulp-sourcemaps": "^1.6.0",
      "gulp-uglify": "^1.5.3",
      "gulp-ui5-preload": "^1.2.0",
      "gulp-util": "^3.0.7",
      "http-proxy-middleware": "^0.17.0",
