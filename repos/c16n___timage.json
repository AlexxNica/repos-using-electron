  "packageJSON": {
    "name": "timage",
    "version": "0.1.0",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "package:osx": "asar pack \"dist/osx/timage-darwin-x64/timage.app/Contents/Resources/app/\" \"dist/osx/timage-darwin-x64/timage.app/Contents/Resources/app.asar\" && rm -rf \"dist/osx/timage-darwin-x64/timage.app/Contents/Resources/app/\"",
      "create-app:osx": "electron-packager . timage --out=dist/osx --platform=darwin --arch=x64 --version=0.33.1 --icon=assets/favicon.icns --ignore=\"(node_modules/electron-*|dist)\"",
      "create-app:lnx": "electron-packager . timage --out=dist/linux --platform=linux --arch=x64 --version=0.33.1 --icon=assets/favicon.icns --ignore=\"(node_modules/electron-*|dist)\"",
      "pack:osx": "electron-builder \"dist/osx/timage-darwin-x64/timage.app\" --platform=osx --out=\"dist/osx\" --config=packager.json",
      "clean:osx": "rm -r dist/osx",
      "clean:lnx": "rm -r dist/linux",
      "build:osx": "npm run clean:osx && npm run create-app:osx && npm run package:osx && npm run pack:osx",
      "build:lnx": "npm run clean:lnx && npm run create-app:lnx"
    },
    "author": "Christin Westermann <christin.westermann@gmail.com> (@_c16n_)",
    "devDependencies": {
      "asar": "^0.8.0",
      "electron-builder": "^2.0.1",
      "electron-installer-dmg": "^0.1.0",
      "electron-packager": "^5.1.0",
      "electron-prebuilt": "^0.33.7",
      "node-blink1": "^0.2.1"
    }
  },
