  "packageJSON": {
    "name": "angular2-webpack",
    "version": "1.0.0",
    "description": "A webpack starter for Angular",
    "scripts": {
      "start": "electron main.js",
      "webstart": "webpack-dev-server --inline --progress --port 8080",
      "test": "karma start",
      "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
      "postinstall": "typings install",
      "distribute-ios": "electron-packager . Tangent --platform=darwin --arch=x64 --out=releases/iOS/ --overwrite --asar",
      "distribute-windows": "electron-packager . Tangent --platform=win32 --arch=x64 --out=releases/Windows/ --overwrite --asar",
      "distribute-linux": "electron-packager . Tangent --platform=linux --arch=x64 --out=releases/Linux/ --overwrite --asar"
    },
