      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "rules": {
      "no-new": 1,
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "ignoreFiles": "bundle/**",
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-group",
            "all-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ],
      "font-family-name-quotes": "single-where-recommended",
      "function-url-quotes": "single",
      "number-leading-zero": "never",
      "string-quotes": "single"
    }
  },
