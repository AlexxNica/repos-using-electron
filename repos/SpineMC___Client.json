    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "export NODE_ENV='dev'; grunt run",
      "debug": "export NODE_ENV='dev'; grunt watcher -v && electron app",
      "win": "grunt watcher & electron app",
      "build": "grunt build; cd build/ && npm run dist; rm -r ../../dist/client;mv dist ../../dist/client; cd ..; rm -r build",
      "dist": "npm run dist:osx && npm run dist:win64",
      "dist:osx": "build --osx",
      "dist:win64": "build --win --x64"
    },
