  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "babel-node bin/compile",
    "lint": "xo",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "better-npm-run dev:client",
    "dev:server": "better-npm-run dev:server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "test-node": "better-npm-run test-node",
    "deploy": "better-npm-run deploy",
    "flow:check": "babel-node bin/flow-check"
  },
  "betterScripts": {
    "dev:client": {
      "command": "babel-node bin/server-dev",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_ENV": "development",
        "NODE_PATH": "./server",
        "DEBUG": "app:*"
      }
    },
    "dev:server": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./server",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start bin/karma",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "test-node": {
      "command": "nyc ava | faucet",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "node:*"
      }
    }
  },
  "ava": {
    "files": [
      "./tests/**/*.ava.js"
    ],
    "failFast": true,
    "tap": true,
    "verbose": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "ignores": [
      "server/middleware/webpack-proxy.js"
    ],
    "envs": [
      "browser",
      "node",
      "es6"
    ],
    "globals": [
      "process.env",
      "NODE_ENV",
      "__CLIENT__",
      "__SERVER__",
      "__DEV__",
      "__PROD__",
      "__TEST__",
      "__DEBUG__",
      "__DEBUG_NEW_WINDOW__",
      "__BASENAME__",
      "__NO_SSR__",
      "webpackIsomorphicTools"
    ]
  },
