{
  "name": "my-evernote",
  "version": "0.0.1",
  "description": "A markedown editor for Evernote build on Electron and Vue.js",
  "main": "main.js",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test --watch",
    "lint": "eslint src test *.js",
    "build": "better-npm-run build",
    "dev": "better-npm-run start-hot",
    "dev-server": "node bin/server.js",
    "build-testing": "better-npm-run build-testing",
    "pack-testing": "better-npm-run pack-testing",
    "pack-testing-all": "better-npm-run pack-testing-all",
    "release": "better-npm-run pack",
    "release-all": "better-npm-run pack-all"
  },
  "betterScripts": {
    "start-hot": {
      "command": "electron ./",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "pack": {
      "command": "node bin/pack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "pack-all": {
      "command": "node bin/pack.js --all",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack-config/production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "pack-testing": {
      "command": "node bin/pack.js",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "pack-testing-all": {
      "command": "node bin/pack.js --all",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "build-testing": {
      "command": "webpack --config webpack-config/testing.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": "gaoqiankun",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.9.1",
    "babel": "^6.3.26",
