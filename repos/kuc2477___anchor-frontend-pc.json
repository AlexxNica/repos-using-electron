    "scripts": {
      "test": "better-npm-run test",
      "check": "better-npm-run check",
      "start": "better-npm-run start",
      "start-hot": "better-npm-run start-hot",
      "hot-server": "better-npm-run hot-server",
      "build": "better-npm-run build",
      "lint": "eslint app test *.js"
    },
    "betterScripts": {
      "watch": {
        "command": "karma start ./karma.conf.js",
        "env": {
          "NODE_ENV": "production"
        }
      },
      "test": {
        "command": "karma start ./karma.conf.js --single-run",
        "env": {
          "NODE_ENV": "production"
        }
      },
      "check": {
        "command": "npm-check"
      },
      "start": {
        "command": "electron ./",
        "env": {
          "NODE_ENV": "production",
          "WINDOW_WIDTH": 1072,
          "WINDOW_HEIGHT": 600
        }
      },
      "start-hot": {
        "command": "electron ./",
        "env": {
          "HOT": true,
          "DEV_SERVER_PORT": 5000,
          "NODE_ENV": "development",
          "WINDOW_WIDTH": 1072,
          "WINDOW_HEIGHT": 600
        }
      },
      "hot-server": {
        "command": "node ./server.js",
        "env": {
          "HOT_SERVER_PORT": 3000
        }
      },
      "build": {
        "command": "webpack --config webpack.config.prod.js --progress --profile --colors",
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    "bin": {
      "electron": "./node_modules/.bin/electron"
    },
