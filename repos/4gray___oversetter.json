  "packageJSON": {
    "name": "Oversetter",
    "version": "0.0.1",
    "author": "4gray",
    "description": "Oversetter is an translation app, which allows you to translate content directly from your menu bar",
    "scripts": {
      "start": "webpack-dev-server --inline --progress --port 8080",
      "test": "karma start",
      "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
      "postinstall": "typings install",
      "prepare": "cp -a electron/. dist",
      "electron": "npm run prepare && electron dist",
      "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist App --platform=darwin --arch=all --out=app --overwrite",
      "dist": "build -mwl --x64 --ia32"
    },
