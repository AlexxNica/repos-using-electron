      "test": "xo",
      "start": "npm run build:js && electron .",
      "out": "npm run build:js && mkdir out && cp main.js out/ && cp package.json out/ && mkdir \"out/app\" && cp app/*.js out/app/ && cp app/index.* out/app/",
