    "url": "https://github.com/akameco/yamada/issues"
  },
  "homepage": "https://github.com/akameco/yamada#readme",
  "dependencies": {
    "array-includes": "^3.0.2",
    "chokidar": "^1.6.0",
    "command-installer": "^0.2.2",
    "lodash.shuffle": "^4.0.0",
    "minimist": "^1.2.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "cpx": "^1.3.2",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.3.2",
    "envify": "^3.4.1",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.3",
    "xo": "^0.16.0"
  },
