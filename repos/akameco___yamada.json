  "packageJSON": {
    "name": "yamada",
    "version": "0.5.1",
    "description": "Image viewer for making happy.",
    "main": "index.js",
    "scripts": {
      "start": "electron .",
      "clean": "rimraf dist",
      "copy": "cpx 'package.json' dist && cpx './src/{*.html,*.png,*.css}' dist",
      "copy:sh": "cp yamada.sh release/yamada-darwin-x64/yamada.app/Contents/Resources/",
      "build": "npm-run-all -s clean copy build:renderer build:main",
      "build:renderer": "browserify ./src/browser.js --im -o ./dist/browser.js",
      "build:main": "browserify ./src/index.js -t [ envify --NODE_ENV production ] --im --no-detect-globals --node -o ./dist/index.js",
      "release:osx": "electron-packager ./dist yamada --out=./release --platform=darwin --arch=x64 --version=1.3.3 --overwrite --asar --icon=./res/yamada.icns",
      "release:win": "electron-packager ./dist yamada --out=./release --platform=win32 --arch=x64 --version=1.3.3 --overwrite --asar --icon=./res/yamada.icon",
      "release": "npm-run-all -s build release:* copy:sh",
      "test": "xo"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/akameco/yamada.git"
    },
    "keywords": [
      "electron",
      "image",
      "viewer"
    ],
    "author": "akameco <akameco.t@gmail.com>",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/akameco/yamada/issues"
    },
    "homepage": "https://github.com/akameco/yamada#readme",
    "dependencies": {
      "array-includes": "^3.0.2",
      "chokidar": "^1.6.0",
      "command-installer": "^0.2.2",
      "lodash.shuffle": "^4.2.0",
      "minimist": "^1.2.0",
      "redux": "^3.5.2",
      "redux-saga": "^0.11.0"
    },
    "devDependencies": {
      "browserify": "^13.1.0",
      "cpx": "^1.3.2",
      "electron-packager": "^7.5.1",
      "electron-prebuilt": "^1.3.3",
      "envify": "^3.4.1",
      "npm-run-all": "^2.3.0",
      "rimraf": "^2.5.4",
      "xo": "^0.16.0"
    },
    "xo": {
      "esnext": true,
      "env": [
        "node",
        "browser"
      ],
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    }
