    "scripts": {
      "postinstall": "cd ./src/server && npm i && cd ../..",
      "start": "npm run build:cached && NODE_ENV=production electron ./",
      "build": "npm run lint && NODE_ENV=production npm run webpack -- --config webpack.dist.js",
      "build:cached": "([ -d 'dist' ] && echo '\nRunning using existing dist files\n') || npm run build",
      "test": "echo TBD",
      "dev": "npm run -s webpack:server & electron ./ & npm run server:dev",
      "dev:browser": "npm run -s webpack:server",
      "dev:mock": "MOCK_API=true npm run -s webpack:server & electron ./",
      "dev:browser:mock": "MOCK_API=true npm run -s webpack:server",
      "server:dev": "cd ./src/server && npm run stop && npm run start:dev && npm run logs",
      "server:stop": "cd ./src/server && npm run stop",
      "webpack": "webpack --progress --stats --colors --display-reasons --display-error-details",
      "webpack:server": "webpack-dev-server --config webpack.dev.js --progress --inline --colors --hot",
      "lint": "eslint --ext .js --ext .jsx ./src/client",
      "analyse": "(NODE_ENV=production npm run webpack -s -- --config webpack.dist.js --json > webpack.stats.json && chalk white bgGreen bold 'OK. Now upload the `webpack.stats.json` file to http://webpack.github.io/analyse') || chalk yellow bgRed bold 'Error occured. Check the `webpack.stats.json` file for details.'",
      "help": "npm-scripts-info",
      "docs": "esdoc -c esdoc.json",
      "outdated": "ncu",
      "clear:config": "rm ~/.config/contractor-dashboard.json && echo 'Cleared' || echo '\nAlready cleared!\n'",
      "clear:repo": "rm -rf ./node_modules && rm -rf ./src/server/node_modules && rm -rf dist && npm run clear:config"
    },
