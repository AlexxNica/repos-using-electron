      "kaku-core": "0.0.5",
      "mdns-js": "^0.5.0",
      "node-itunes-rss-data": "^1.1.1",
      "node-soundcloud": "0.0.5",
      "node-uuid": "^1.4.7",
      "pouchdb": "^3.6.0",
      "react": "^15.3.1",
      "react-dom": "^15.3.1",
      "react-emoji": "^0.4.4",
      "react-tooltip": "^1.2.1",
      "reactfire": "^0.7.0",
      "request": "^2.69.0",
