    "scripts": {
      "clean": "npm cache clean && rimraf node_modules doc typings coverage protractorReport dist",
      "typings-install": "typings install",
      "postinstall": "npm run typings-install",
      "start": "node ./backend/api.js",
      "test": "karma start",
      "watch:test": "npm run test -- --auto-watch --no-single-run",
      "coverage": "http-server -c-1 -o -p 9875 ./coverage/report",
      "test:coverage": "npm run test && npm run coverage",
      "prebuild": "rimraf dist",
      "build": "webpack --progress --profile --colors --display-error-details --display-cached",
      "pretest": "npm run lint",
      "prepack": "npm run build && rimraf package",
      "pack": "electron-packager ./dist OPEN --platform=win32 --arch=x64 --overwrite --out=package --icon=./src/favicon.ico --version-string.ProductName=OPEN",
      "postpack": "cd package/OPEN-win32-x64/resources/app && npm install",
      "preinstaller": "npm run pack",
      "installer": "node installer.js",
      "lint": "tslint src/**/*.ts",
      "docs": "typedoc --options typedoc.json --exclude **/*.spec.ts ./src/",
      "webdriver:update": "webdriver-manager update",
      "webdriver:start": "webdriver-manager start",
      "pree2e": "npm run webdriver:update -- --standalone",
      "e2e": "protractor",
      "e2e:live": "protractor --elementExplorer"
    },
