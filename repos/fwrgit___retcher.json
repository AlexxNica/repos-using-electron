    "version": "0.6.1",
    "main": "main.js",
    "description": "GUI for easily create boot media directly from resin.io applications",
    "scripts": {
      "postinstall": "bower i",
      "start": "./node_modules/.bin/electron .",
      "test": "echo \"Error: no test specified\" && exit 1",
      "pack": "build",
      "dist": "build"
