      "io",
      "burn",
      "flash",
      "image",
      "img",
      "app",
      "base"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/fwrgit/retcher#readme",
    "build": {
      "appDir": "nope",
      "app-bundle-id": "it.fwr.retcher",
      "app-category-type": "public.app-category.utilities",
      "osx": {
        "title": "Retcher",
        "contents": [
          {
            "x": 410,
            "y": 150,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      }
    },
    "packageIgnore": [
      "app/components/core/config/config.json",
      "app/.tmp/*.img",
      "app/.tmp/cache/*"
    ],
