    "main": "dest/main.js",
    "scripts": {
      "start": "gulp",
      "clean": "rm -rf ./release",
      "clean:osx": "rm -rf ./release/osx",
      "clean:win": "rm -rf ./release/win",
      "pack": "npm run pack:osx && npm run build:win",
      "pack:osx": "npm run clean:osx && electron-packager . \"SqMd\" --out=release/osx --platform=darwin --arch=x64 --version=0.36.7",
      "pack:win": "npm run clean:win && electron-packager . \"SqMd\" --out=release/win --platform=win32 --arch=ia32 --version=0.36.7"
    },
