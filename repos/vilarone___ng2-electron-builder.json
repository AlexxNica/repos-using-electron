  "packageJSON": {
    "name": "ng2-electron",
    "version": "0.1.0",
    "license": "MIT",
    "author": "David Vilar√≥",
    "angular-cli": {},
    "directories": {
      "output": "release",
      "app": "dist",
      "buildResources": "build"
    },
    "build": {
      "appId": "app.electron.ng2"
    },
    "scripts": {
      "electrontsc": "tsc -p electron/",
      "start": "ng serve",
      "lint": "tslint \"src/**/*.ts\"",
      "test": "ng test",
      "pree2e": "webdriver-manager update",
      "e2e": "protractor",
      "dev": "ng build",
      "build": "ng build --prod",
      "prepare:dev": "npm run dev && npm run electrontsc && ncp electron/package.json dist/package.json",
      "prepare:prod": "npm run build && npm run electrontsc && ncp electron/package.json dist/package.json",
      "prepare": "npm run prepare:dev",
      "electron:dev": "npm run prepare:dev && electron dist",
      "electron:prod": "npm run prepare:prod && electron dist",
      "electron": "npm run electron:dev",
      "package:dev": "npm run prepare:dev && electron-packager dist App --out=release --overwrite",
      "package:prod": "npm run prepare:prod && electron-packager dist App --out=release --asar=true --overwrite",
      "package": "npm run package:dev",
      "release": "npm run package:prod && build"
    },
    "private": true,
