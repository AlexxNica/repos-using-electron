  "packageJSON": {
    "name": "desktop-boilerplate",
    "description": "Boilerplate for desktop applications",
    "author": "Phil Ostler <phil@basscamp.io>",
    "version": "1.0.0",
    "license": "SEE LICENSE.md",
    "private": true,
    "engines": {
      "node": ">=6"
    },
    "main": "app/main.js",
    "scripts": {
      "clean": "npm run clean:build -s && npm run clean:package -s",
      "clean:build": "rimraf app && mkdir app",
      "clean:package": "rimraf package",
      "prebuild": "npm run clean:build -s",
      "build": "npm run build:app && npm run build:main",
      "build:app": "webpack --config config/webpack/production.app.config.js",
      "build:main": "webpack --config config/webpack/production.main.config.js",
      "prepackage": "npm run clean:package -s && npm run build",
      "package": "node config/electron-packager/package.js",
      "start": "concurrently --kill-others --raw \"npm run start:server\" \"npm run start:electron\"",
      "start:electron": "NODE_ENV=development electron --require babel-register src/main.js",
      "start:server": "node config/webpack/development.server.js",
      "analyse:app": "webpack --config config/webpack/production.app.config.js --json --profile > profile_app.json && open https://webpack.github.io/analyse",
      "analyse:main": "webpack --config config/webpack/production.main.config.js --json --profile > profile_main.json && open https://webpack.github.io/analyse",
      "test": "npm run test:unit && npm run test:functional",
      "test:unit": "karma start config/karma/unit.config.js",
      "test:functional": "mocha --opts config/mocha/functional.config.opts"
    },
    "dependencies": {
      "normalize.css": "4.1.1",
      "react": "15.1.0",
      "react-css-modules": "3.7.6",
