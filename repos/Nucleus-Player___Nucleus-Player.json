  "packageJSON": {
    "name": "nucleus.player",
    "productName": "Nucleus Player",
    "version": "1.0.0",
    "description": "An electron wrapper for Google Play Music",
    "main": "src/index.js",
    "scripts": {
      "build": "gulp build",
      "lint": "eslint ./src/ ./test/",
      "make:win": "npm run package:win && grunt build:win",
      "package": "gulp package",
      "package:darwin": "gulp package-darwin && npm i",
      "package:win": "gulp package-win && npm i",
      "release:win": "release-nucleus --nucleus-path=./ --release-type=win",
      "release:darwin": "release-nucleus --nucleus-path=./ --release-type=darwin",
      "test": "npm run lint && npm run test-unit",
      "test-unit": "electron-mocha ./test -r test/support/prerun.js",
      "start": "electron .",
      "watch": "npm run build && gulp watch"
    },
