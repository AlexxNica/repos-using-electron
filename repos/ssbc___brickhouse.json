    "pull-pushable": "^1.1.4",
    "pull-stream": "^2.27.0",
    "scuttlebot": "~7.0.2",
    "ssb-config": "^1.0.3",
    "ssb-keys": "^4.0.3",
    "ssb-ref": "^2.2.1",
    "stream-to-pull-stream": "^1.6.1"
  }
