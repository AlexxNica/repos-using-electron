  "packageJSON": {
    "name": "sulu",
    "description": "A hackable file manager",
    "version": "0.0.13",
    "homepage": "https://github.com/s-a/sulu",
    "keywords": [],
    "license": "",
    "private": true,
    "repository": {
      "type": "git",
