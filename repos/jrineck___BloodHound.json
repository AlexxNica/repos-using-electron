      "babel-preset-stage-0": "^6.5.0",
      "concurrently": "^2.2.0",
      "cross-env": "^2.0.0",
      "electron": "^1.4.3",
      "express": "^4.14.0",
      "webpack": "^1.13.1",
      "webpack-dev-middleware": "^1.6.1",
      "webpack-hot-middleware": "^2.12.1",
      "webpack-target-electron-renderer": "^0.4.0"
    },
    "dependencies": {
      "bootstrap": "^3.3.6",
      "bootstrap-3-typeahead": "^4.0.1",
      "configstore": "^2.0.0",
      "dagre": "^0.7.4",
      "eventemitter2": "^2.0.0",
      "jquery": "^2.2.4",
      "linkurious": "^1.5.1",
      "mustache": "^2.2.1",
      "neo4j-driver": "^1.0.4",
      "react": "^15.3.1",
      "react-addons-css-transition-group": "^15.3.1",
      "react-bootstrap": "^0.30.3",
      "react-dom": "^15.3.1",
      "react-if": "^2.1.0"
    }
