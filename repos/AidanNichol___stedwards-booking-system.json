  "packageJSON": {
    "name": "webpack-StEds-Redux",
    "version": "0.1.0",
    "description": "A boilerplate for running a Webpack workflow in Node express",
    "main": "index.js",
    "scripts": {
      "electron": "ENV=development electron index.js",
      "electronh": "ENV=development electron -h",
      "electronp": "ENV=production electron index.js",
      "electronServer": "babel-node server.js",
      "electronTest": "babel-node server.js",
      "start": "concurrently -k 'npm run electronServer' 'npm run electron'",
      "dev": "webpack-dev-server",
      "build": "webpack --config webpack.production.config.js",
      "test": "NODE_PATH=app ava",
      "couchBackup": "couchbackup --url http://localhost:5984 --db bookings > backup.txt",
      "couchRestore": "cat backup.txt | couchrestore",
      "couchApp:bookings": "cd ./couchApp/bookings && pwd && couchApp push",
      "watch:bookings": "watch 'npm run couchApp:bookings' couchApp/bookings/",
      "couchApp:members": "cd ./couchApp/members && pwd && couchApp push",
      "watch:members": "watch 'npm run couchApp:members' couchApp/members/",
      "couchApp:walks": "cd ./couchApp/walks && pwd && couchApp push",
      "watch:walks": "watch 'npm run couchApp:walks' couchApp/walks/",
      "couchApp:credits": "cd ./couchApp/credits && pwd && couchApp push",
      "watch:credits": "watch 'npm run couchApp:credits' couchApp/credits/",
      "couchApp": "npm run couchApp:walks && npm run couchApp:credits && npm run couchApp:bookings && npm run couchApp:members",
      "couchApp:watch": "concurrently -k 'npm run watch:walks' 'npm run watch:credits' 'npm run watch:members' 'npm run watch:bookings'",
      "couchDB": "open -a 'Apache couchDB'",
      "server": "BABEL_ENV=development webpack-dev-server --hot --devtool eval --progress --colors --content-base build",
      "startXXX": "concurrently -k 'npm run server' 'npm run couchDB'",
      "devXXX": "concurrently -k 'npm run server' 'npm run couchDB' 'npm run couchApp:watch'"
    },
    "ava": {
      "files": [
        "test/*.js"
      ],
      "source": [
        "**/*.{js,jsx}",
        "!dist/**/*"
      ],
      "verbose": true,
      "concurrency": 5,
      "require": [
        "babel-register",
        "ignore-styles"
      ],
      "babel": "inherit"
    },
    "babel": {
      "presets": [
        "es2015",
