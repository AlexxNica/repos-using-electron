    "scripts": {
      "start": "gulp; NODE_ENV=development electron ./compile",
      "fresh-start": "npm run dump_db; npm run start",
      "build": "gulp dist; cp -R node_modules ./compile",
      "package": "node ./build/package.js --platform=darwin",
      "sign": "node ./build/sign.js",
      "zip": "cd build/releases/Anansi-darwin-x64 && zip -r -y Anansi.zip Anansi.app",
      "dist": "npm run build; npm run package; npm run sign; npm run zip",
      "publish-stable": "npm run dist; gulp upload-stable; node build/env.js --target=stable",
      "publish-beta": "npm run dist; gulp upload-beta; node build/env.js --target=beta",
      "preversion": "npm test",
      "postversion": "npm run dist",
      "test": "rm db.json; NODE_ENV=test mocha --require babel-core/register --reporter dot --timeout 10000 spec/controller",
      "linter": "eslint ./src",
      "dump_db": "rm ~/Library/Application\\ Support/Electron/db.json"
    },
