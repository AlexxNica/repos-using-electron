{
  "name": "hackertunes",
  "version": "1.0.0",
  "description": "A hackable music player using node",
  "main": "app.js",
  "scripts": {
    "start": "electron ./index.js",
    "postinstall": "echo 'Rebuilding using electron: ' && electron-rebuild",
    "fixbuild": "npm run postinstall",
    "test:cover": "nyc --extension .jsx --reporter=html --reporter=clover --reporter=lcov npm run test && nyc report",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "NODE_ENV='test' mocha test --recursive --compilers js:babel-core/register",
    "doc": "jsdoc -c .jsdoc.js -r -t ./node_modules/ink-docstrap/template -d docs -R README.md"
  },
