    "main": "index.js",
    "scripts": {
      "electron": "npm run ts-node -- ./source/scripts/electron.ts",
      "elm-make": "npm run ts-node -- ./source/scripts/elm-make.ts",
      "nodemon": "node ./node_modules/nodemon/bin/nodemon.js",
      "watch": "npm run nodemon -- --watch source --ext elm,ts,tsx --exec \"npm run maybe-format && npm run update-README && npm start\" --ignore source/client/elm/Pages/Index.elm",
      "format": "./binaries/elm-format --yes source/client/elm/*.elm",
      "maybe-format": "npm run ts-node -- ./source/scripts/maybe-format.ts",
      "build": "npm run elm-make && npm run webpack",
      "webpack": "node ./node_modules/webpack/bin/webpack.js",
      "start": "npm run elm-make && npm run webpack && npm run electron",
      "test": "echo \"Error: no test specified\" && exit 1",
      "tsc": "node ./node_modules/typescript/lib/tsc.js",
      "update-README": "npm run ts-node -- ./source/scripts/update-README.ts",
      "ts-node": "node ./node_modules/ts-node/dist/bin.js"
    },
    "keywords": [
      "elm",
      "react",
      "typescript",
