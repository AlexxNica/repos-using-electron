  "packageJSON": {
    "name": "patchwork-nodekit",
    "version": "0.1.0",
    "description": "Fork of P2P Messaging App ssbc/patchwork adapted for Node and NodeKit",
    "main": "_offgrid/server/server.js",
    "repository": {
      "type": "git",
      "url": "https://github.com/offgridnetworks/patchwork-nodekit.git"
    },
    "scripts": {
      "desktop": "electron .",
      "clean": "rimraf _offgrid/static && rimraf _offgrid/dist && rimraf _offgrid/public/fonts && rimraf _offgrid/public/img",
      "copy": "mkdir -p _offgrid/plugins && cp ./node_modules/scuttlebot/api.md ./_offgrid && cp ./node_modules/scuttlebot/plugins/*.md ./_offgrid/plugins && mkdir -p _offgrid/dist && cp -r ./node_modules/mime/types ./_offgrid/dist/ && mkdir -p _offgrid/public && cp -r ./ui/fonts _offgrid/public && cp -r ./ui/img _offgrid/public && cp ./_offgrid/client/image-input.jsx ./ui/com/form-elements/ && cp ./_offgrid/client/lib/muxrpc-ipc.js ./ui/lib && rm -rf ./node_modules/level",
      "prepublish": "npm run build:ui",
      "build:ui": "mkdir -p ./ui/css && lessc ./ui/less/main.less ./ui/css/main.css",
      "build:webpack": "NODE_ENV=production webpack --config _offgrid/webpack.config.prod.js",
      "build:server": "NODE_ENV=production webpack --config _offgrid/webpack.config.server.js",
      "build:prod": "npm run clean && npm run copy && npm run build:webpack",
      "start": "node _offgrid/dist/prodserver",
      "server": "node _offgrid/server/server",
      "dev": "NODE_ENV=development node _offgrid/server/devServer"
    },
