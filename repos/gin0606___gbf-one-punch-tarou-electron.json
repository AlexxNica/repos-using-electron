  "packageJSON": {
    "name": "gbf-one-punch-tarou-electron",
    "private": true,
    "version": "1.0.3",
    "productName": "OnePunchTarou",
    "description": "風ホーク死ね",
    "main": "main.js",
    "scripts": {
      "start": "NODE_ENV=development electron main.js",
      "build": "NODE_ENV=development browserify -t babelify -o bundle.js src/main.jsx",
      "watch": "NODE_ENV=development watchify -t babelify -o bundle.js src/main.jsx -v",
      "test": "mocha",
      "lint": "eslint src/**/*.{js,jsx}",
      "release:build": "NODE_ENV=production browserify -t babelify -o bundle.js src/main.jsx",
      "package:osx": "NODE_ENV=production node build.js darwin",
      "package:win": "NODE_ENV=production node build.js win32",
      "package:osx:zip": "cd build/Release && zip -P $TAROU_ZIP_PASS --quiet -r OnePunchTarou-darwin-x64.zip OnePunchTarou-darwin-x64",
      "package:win:zip": "cd build/Release && zip -P $TAROU_ZIP_PASS --quiet -r OnePunchTarou-win32-x64.zip OnePunchTarou-win32-x64 && zip -P $TAROU_ZIP_PASS --quiet -r OnePunchTarou-win32-ia32.zip OnePunchTarou-win32-ia32",
      "package": "NODE_ENV=production npm-run-all release:build package:osx package:win package:osx:zip package:win:zip"
    },
    "author": "gin0606",
    "license": "MIT",
