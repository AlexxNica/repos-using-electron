  "packageJSON": {
    "name": "gitar",
    "productName": "Gitar",
    "description": "Open-Source guitar tabs player & editor.",
    "version": "0.0.1",
    "main": "dist/main.js",
    "author": {
      "name": "rkostrzewski",
      "email": "poczta.raf.kos@gmail.com",
      "url": "https://github.com/rkostrzewski"
    },
