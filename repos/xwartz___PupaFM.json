    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.5.2",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "doubanfm-sdk": "^2.0.0",
