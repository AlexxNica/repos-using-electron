      ],
      "rules": {
        "yoda": "off",
        "semi": [
          "error",
          "always"
        ],
        "no-unused-vars": "error",
        "no-extra-semi": "error",
        "semi-spacing": [
          "error",
          {
            "before": false,
            "after": true
          }
