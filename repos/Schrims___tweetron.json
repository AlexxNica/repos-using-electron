    "scripts": {
      "test": "xo",
      "start": "electron .",
      "build": "npm run build:osx && npm run build:linux && npm run build:windows",
      "build:osx": "electron-packager . --overwrite --asar --out=dist --ignore='^icons$' --prune --platform=darwin --arch=x64 --icon=icons/Icon.icns --app-bundle-id=com.sindresorhus.anatine --sign='Developer ID Application: Sindre Sorhus (YG56YK5RN5)' --app-version=$npm_package_version && cd dist/Anatine-darwin-x64 && zip -ryXq9 ../Anatine-osx-${npm_package_version}.zip Anatine.app",
      "build:linux": "electron-packager . --overwrite --out=dist --ignore='^icons$' --prune --platform=linux --arch=x64 --app-bundle-id=com.sindresorhus.anatine --app-version=$npm_package_version && cd dist/Anatine-linux-x64/ && zip -ryq9 ../Anatine-linux-${npm_package_version}.zip *",
      "build:windows": "electron-packager . --overwrite --asar --out=dist --ignore='^icons$' --prune --platform=win32 --arch=ia32 --icon=icons/Icon.ico --version-string.ProductName=$npm_package_productName --app-version=$npm_package_version && cd dist/Anatine-win32-ia32 && zip -ryq9 ../Anatine-windows-${npm_package_version}.zip *"
    },
    "dependencies": {
      "electron-config": "^0.1.1",
      "electron-context-menu": "^0.1.0",
      "electron-debug": "^1.0.0",
      "electron-dl": "^1.0.0"
    },
    "devDependencies": {
      "electron-packager": "^7.0.0",
