  "name": "bl-agent-electron",
  "version": "1.0.0",
  "description": "BL Agent App",
  "main": "./index.js",
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",
    "pack": "npm run clean && npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run clean:osx && electron-packager . \"BLAgent\" --out=dist/osx --platform=darwin --arch=x64 --version=0.36.1 --icon=assets/osx/mount.icns",
    "pack:win": "npm run clean:win && electron-packager . \"BLAgent\" --out=dist/win --platform=win32 --arch=ia32 --version=0.36.1 --icon=assets/win/mount.ico",
    "build": "npm run build:osx && npm run build:win",
    "build:osx": "npm run pack:osx && electron-builder \"dist/osx/BLAgent.app\" --platform=osx --out=\"dist/osx\" --config=builder.json",
    "build:win": "npm run pack:win && electron-builder \"dist/win/BLAgent-win32\" --platform=win --out=\"dist/win\" --config=builder.json"
  },
