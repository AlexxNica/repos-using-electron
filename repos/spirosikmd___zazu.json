    "scripts": {
      "test": "karma start",
      "start": "export ENV=development && concurrently --kill-others \"webpack-dev-server --inline --progress --port 8080\" \"electron .\"",
      "clean": "rimraf dist",
      "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
      "copy": "cp package.json main.js dist && cp -r node_modules dist/node_modules",
      "package": "electron-packager dist Zazu --asar --asar-unpack=protocol-link.html --platform=darwin --arch=x64 --version=1.4.2 --overwrite --prune=true --out=out --ignore=\"node_modules\"",
      "deploy": "npm test && npm run clean && npm run build && npm run copy && npm run package"
    },
