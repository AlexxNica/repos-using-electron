  "packageJSON": {
    "name": "translation_core",
    "productName": "translationCore",
    "version": "0.1.2",
    "description": "A bridge between TS and TM",
    "main": "main.js",
    "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "compile": "babel out --out-dir out",
      "compile-modules": "babel modules --out-dir modules",
      "build-win": "xcopy src out /Y /E && npm run compile && node scripts/build-win.js",
      "build-mac": "cp -R src/ out/ && npm run compile && build --mac",
      "build-lin": "cp src/* out/ && npm run compile && build --linux",
      "start": "electron ."
    },
    "build": {
      "appId": "wa.translation_core.desktop",
      "category": "public.app-category.bible.tools"
    },
