      "png-to-ico": "^1.0.1",
      "postcss-cssnext": "^2.8.0",
      "postcss-loader": "^0.13.0",
      "style-loader": "^0.13.1",
      "url-loader": "^0.5.7",
      "webpack": "^1.13.2",
      "webpack-target-electron-renderer": "^0.4.0"
