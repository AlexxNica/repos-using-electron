  "packageJSON": {
    "name": "probe-electron",
    "productName": "Lemonce Editor",
    "version": "1.1.2",
    "description": "lemonce desktop application",
    "main": "index.js",
    "scripts": {
      "test": "mocha",
      "e": "cross-env NODE_ENV=development electron .",
      "dev": "npm run copy && webpack --config webpack.dev.js",
      "start": "npm run dev",
      "lint": "eslint *.js",
      "build:windows": "node build/win32.js && npm run build:zip",
      "build:zip": "cd output && bestzip Lemonce_win_%npm_package_version%.zip Lemonce-win32-ia32",
      "build:ico": "png-to-ico static/Lemonce.png > static/Lemonce.ico",
      "build:macos": "electron-packager . --overwrite --out=output --asar --prune --ignore='server/index' --platform=darwin --arch=x64 --app-bundle-id=com.lemonce.probe --app-version=$npm_package_version && cd 'output/Lemonce Editor-darwin-x64' && zip -ryXq9 ../Lemonce_osx_${npm_package_version}.zip 'Lemonce Editor.app'",
      "build:exe": "node build/inno.js",
      "copy": "mkdirp dist/viewer && ncp node_modules/probe-simple-report/dist/report.min.js dist/viewer/build.js"
    },
    "repository": "https://github.com/lemonce/probe-electron",
    "keywords": [],
    "license": "GPL-3.0",
    "homepage": "https://github.com/lemonce/probe-electron#readme",
