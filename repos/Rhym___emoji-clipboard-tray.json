    "scripts": {
      "start": "electron docs/bundle.js",
      "watch": "watchify src/javascripts/index.jsx -t babelify -o docs/bundle.js --debug --verbose",
      "build": "webpack --config webpack.config.js && gulp",
      "production": "webpack --config webpack.config.js && gulp",
      "package": "yarn package:macos",
      "package:macos": "./node_modules/.bin/electron-packager . --overwrite --asar --out=dist --platform=darwin --app-version=${npm_package_version} && cd dist/EmojiPicker-darwin-x64 && zip -ryXq9 ../EmojiPicker-macOS-${npm_package_version}.zip EmojiPicker.app",
      "package:linux": "./node_modules/.bin/electron-packager . --overwrite --asar --out=dist --platform=linux --app-version=${npm_package_version} && cd dist/EmojiPicker-linux-x64/ && zip -ryq9 ../EmojiPicker-linux-${npm_package_version}.zip *",
      "package:windows": "./node_modules/.bin/electron-packager . --overwrite --asar --out=dist  --prune --platform=win32 --arch=ia32 --version-string.ProductName=${npm_package_productName} --app-version=${npm_package_version} && cd dist/EmojiPicker-win32-ia32 && zip -ryXq9 ../EmojiPicker-windows-${npm_package_version}.zip *"
    },
