  "packageJSON": {
    "name": "logi-pharma-front",
    "version": "0.0.1",
    "description": "Our product GUI",
    "main": "src/main.js",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/ekougs/logi-pharma-front"
    },
    "scripts": {
      "tsc": "tsc",
      "tsc:w": "tsc -w",
      "build:css": "node-sass --output src/app src/app ; node-sass src/main.scss src/main.css",
      "watch:scss": "nodemon -e scss -x \"npm run build:css\"",
      "build": "npm-run-all -p tsc build:css ",
      "run:pharma-app": "electron src/main.js",
      "prestart-common": "npm run build",
      "start-common": "npm-run-all -p tsc:w watch:scss run:pharma-app",
      "start": "npm run start:dev",
      "start:dev": "ENV=DEV npm run start-common",
      "start:prod": "ENV=PROD npm run start-common",
      "http-server:test": "node config/test-server.js",
      "preserver:test": "npm build",
      "serve:test": "npm-run-all -p tsc:w watch:scss http-server:test",
      "daemon-http-server:test": "forever start config/test-server.js",
      "protractor:local": "protractor config/e2e.protractor.conf.js",
      "pree2e": "npm run build",
      "e2e": "npm-run-all -p daemon-http-server:test protractor:local ; forever stop config/test-server.js",
      "pretest": "npm run build",
      "test": "karma start config/karma.conf.js",
      "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
      "coverage": "http-server -c-1 -o -p 9875 ./coverage",
      "predist": "npm run build",
      "dist": "node config/create-dist.js",
      "prepackage": "del -f dist logipharma-*/ && npm run dist",
      "postpackage": "del dist",
      "package": "electron-packager dist logipharma",
      "package:mac": "npm run package -- --platform=darwin --arch=x64",
      "package:windows": "npm run package -- --platform=win32 --arch=ia32"
    },
