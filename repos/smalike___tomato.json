  "packageJSON": {
    "name": "tomato",
    "version": "1.0.0",
    "description": "ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/",
    "main": "main.js",
    "scripts": {
      "start": "electron ./app",
      "watch": "webpack --watch",
      "prepackager": "rm -rf app && webpack --config webpack.config.production.js && rm -rf dist",
      "packager": "electron-packager ./app Hosts --all --overwrite --out=./dist --asar --app-version=0.1.1 --version=1.3.6",
      "dist": "build"
    },
    "build": {
      "appId": "org.smalike.hosts",
      "category": "hosts.manager",
      "dmg": {
        "contents": [
          {
            "x": 410,
            "y": 150,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 130,
            "y": 150,
            "type": "file"
          }
        ]
      },
      "win": {
        "remoteReleases": "",
        "publish": [
          "github",
          "bintray"
        ]
      },
      "linux": {
        "publish": null,
        "target": [
          "deb",
          "AppImage"
        ]
      }
    },
    "license": "ISC",
    "devDependencies": {
