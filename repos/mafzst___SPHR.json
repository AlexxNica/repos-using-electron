    "name": "react-electron",
    "version": "0.2.0",
    "description": "An electron desktop app to live reload the Symfony Profiler during development",
    "contributors": "Nicolas Perraut <contact@nicolas-perraut.fr>",
    "license": "GNU GPL v3",
    "keywords": [
      "symfony",
