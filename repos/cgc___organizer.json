  "packageJSON": {
    "name": "scitran-organizer",
    "productName": "Organizer",
    "version": "0.0.1",
    "description": "SciTran Data Organizer",
    "main": "main.js",
    "scripts": {
      "start": "electron main.js",
      "start-hot": "cross-env HOT=1 electron main.js",
      "dev": "concurrently --kill-others \"npm run watch\" \"npm run start-hot\"",
      "lint": "eslint app *.js",
      "clean": "rm -rf ./build ./css ./dist",
      "clean:osx": "rm -rf ./dist/osx",
      "clean:win": "rm -rf ./dist/win",
      "pack": "npm run pack:osx && npm run pack:win",
      "pack:build": "npm run clean && npm run lint && npm run webpack",
      "pack:osx": "npm run pack:build && electron-packager . --out=dist/osx --platform=darwin --arch=x64 --icon=assets/osx/logo.icns --prune --ignore=assets --ignore=dist --ignore=scss",
      "pack:win": "npm run pack:build && electron-packager . --out=dist/win --platform=win32 --arch=ia32 --icon=assets/win/logo.ico --prune--ignore=assets --ignore=dist --ignore=scss",
      "webpack": "webpack",
      "watch": "webpack-dev-server --hot --inline",
      "node": "node"
    },
    "repository": "https://github.com/scitran/organizer",
    "keywords": [
      "SciTran",
      "Data",
      "Organizer",
      "DICOM",
      "NIfTI",
      "BIDS",
      "OpenfMRI"
    ],
    "license": "MIT",
    "homepage": "https://github.com/scitran/organizer#readme",
    "devDependencies": {
      "concurrently": "^2.2.0",
      "cross-env": "^1.0.8",
      "css-loader": "^0.24.0",
      "electron-packager": "^7.0.7",
      "electron-prebuilt": "^1.3.4",
      "eslint": "^2.13.1",
