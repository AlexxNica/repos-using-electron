      "json-loader": "^0.5.4",
      "node-sass": "^3.10.1",
      "raw-loader": "^0.5.1",
      "sass-loader": "^4.0.2",
      "style-loader": "^0.13.1",
      "webpack": "^1.13.2",
      "webpack-dev-server": "^1.15.0"
    },
    "dependencies": {
      "angular": "^1.5.8",
      "angular-ui-router": "^0.3.1",
      "css-loader": "^0.24.0",
      "archiver": "^1.0.1",
      "dicom-parser": "^1.7.4",
      "request": "^2.74.0",
      "rx": "^4.1.0"
    }
