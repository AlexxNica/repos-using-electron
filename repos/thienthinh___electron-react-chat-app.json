    "scripts": {
      "test": "xo",
      "start": "cross-env HOT=1 NODE_ENV=development electron -r babel-register main.development.js",
      "server": "node -r babel-register server.js",
      "dev": "concurrently --kill-others \"npm run server\" \"npm start\"",
      "build-main": "cross-env NODE_ENV=production node -r babel-register webpack --config webpack.config.electron.js --progress --profile --colors",
      "build-renderer": "cross-env NODE_ENV=production node -r babel-register webpack --config webpack.config.production.js --progress --profile --colors",
      "build": "npm run build-main && npm run build-renderer",
      "package": "cross-env NODE_ENV=production node -r babel-register package.js",
      "package-all": "npm run package -- --all"
    },
