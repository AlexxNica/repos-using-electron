  "packageJSON": {
    "name": "fastatic-app",
    "version": "0.1.0",
    "description": "A Graphical User Interface for the Fastatic NodeJS module. Note that this app is still in beta.",
    "title": "Fastatic App",
    "main": "main.js",
    "scripts": {
      "prestart": "lessc ui/styles.less ui/styles.css --source-map --source-map-rootpath=ui/ & riot ui/components ui/compiled-tags.js --ext html",
      "start": "NODE_ENV=development nodejs-dashboard electron .",
      "lint": "eslint",
      "prepackage": "rimraf releases && icon-gen --input assets/icon.svg --output ./releases/icons --report",
      "package": "electron-packager . 'Fastatic App' --out=releases --overwrite --icon=./releases/icons/app.icns --asar"
    },
    "repository": "https://github.com/voorhoede-labs/fastatic-app",
    "homepage": "https://github.com/voorhoede-labs/fastatic-app#readme",
