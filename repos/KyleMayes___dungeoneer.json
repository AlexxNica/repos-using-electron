  "packageJSON": {
    "name": "dungeoneer",
    "version": "0.0.1",
    "description": "An application for running 5E Dungeons & Dragons® games.",
    "repository": {
      "url": "git@github.com:KyleMayes/dungeoneer.git",
      "type": "git"
    },
    "author": "Kyle Mayes <kyle@mayeses.com>",
    "license": "Apache-2.0",
    "scripts": {
      "build:css": "node-sass --include-path src -r src -o target",
      "watch:css": "node-sass --watch --include-path src -r src -o target",
      "build:html": "cpx src/**/*.html target",
      "watch:html": "cpx --watch src/**/*.html target",
      "build:js": "tsc",
      "watch:js": "tsc --watch",
      "build:vendor": "cpx node_modules/zone.js/dist/zone.js target/vendor && cpx node_modules/reflect-metadata/Reflect.js target/vendor && cpx node_modules/monaco-editor/min/** target/vendor/monaco-editor/min",
      "watch:vendor": "npm run build:vendor",
      "clean": "rimraf target",
      "build": "concurrently \"npm run build:css\" \"npm run build:html\" \"npm run build:js\" \"npm run build:vendor\"",
      "watch": "concurrently \"npm run watch:css\" \"npm run watch:html\" \"npm run watch:js\" \"npm run watch:vendor\"",
      "start:build": "npm run clean && npm run build && electron target",
      "start:watch": "npm run clean && npm run build && concurrently \"npm run watch\" \"electron target\""
    },
    "dependencies": {
      "@angular/common": "2.1.0",
      "@angular/compiler": "2.1.0",
      "@angular/core": "2.1.0",
      "@angular/forms": "2.1.0",
      "@angular/http": "2.1.0",
      "@angular/platform-browser": "2.1.0",
      "@angular/platform-browser-dynamic": "2.1.0",
      "@angular/router": "3.1.0",
      "monaco-editor": "0.7.0",
      "reflect-metadata": "0.1.8",
      "rxjs": "5.0.0-beta.12",
      "zone.js": "0.6.23"
    },
    "devDependencies": {
      "@types/core-js": "0.9.34",
      "@types/electron": "1.4.25",
      "concurrently": "3.1.0",
      "core-js": "2.4.1",
      "cpx": "1.5.0",
      "electron": "1.4.4",
      "electron-reload": "1.0.2",
      "node-sass": "3.10.1",
      "rimraf": "2.5.4",
      "typescript": "2.0.3"
    }
  },
