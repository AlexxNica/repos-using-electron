  "packageJSON": {
    "name": "app",
    "version": "0.1.0",
    "description": "elecron template",
    "license": "UNLICENSED",
    "main": "built/main/main.js",
    "scripts": {
      "package": "node tools/pack.app",
      "clear:built": "node tools/clear built",
      "clear:release": "node tools/clear release",
      "build:main": "cross-env NODE_ENV=production babel src/main -d built/main",
      "build:rend": "cross-env NODE_ENV=production webpack --config=tools/webpack.build",
      "build": "npm run build:rend && npm run build:main",
      "start": "electron .",
      "server:hot": "webpack-dev-server --config=tools/webpack.server",
      "start:hot": "babel src/main -d built/main && cross-env HOT=1 electron ."
    },
    "packager": {
      "appCopyright": "Copyright",
      "win32metadata": {
        "CompanyName": "CompanyName",
        "FileDescription": "FileDescription",
        "OriginalFilename": "OriginalFilename",
        "ProductName": "ProductName",
        "InternalName": "InternalName"
      },
      "icon": "res/icon/app.ico",
      "out": "release",
      "platform": "win32",
      "arch": "ia32",
      "asar": true,
      "prune": true,
      "overwrite": true,
      "removeLisenceFile": true,
      "ignore": [
        "^/src($|/)",
        "^/release($|/)",
        "^/.idea($|/)",
        "^/tools($|/)",
        "^/.gitignore"
      ]
    },
    "babel": {
      "presets": [
        "stage-0",
        "es2015"
      ],
