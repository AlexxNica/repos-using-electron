    "devDependencies": {
      "electron-packager": "^8.0.0",
      "electron-prebuilt": "^1.2.0",
      "fs-extra": "^0.30.0",
      "gulp": "^3.9.1",
      "gulp-json-modify": "^1.0.2",
      "icon-gen": "~1.0.5"
    },
    "build": {
      "appId": "ballast",
      "app-category-type": "public.app-category.productivity",
