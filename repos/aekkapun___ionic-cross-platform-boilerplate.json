  "packageJSON": {
    "name": "ionic-cross-platform-boilerplate",
    "title": "Cross platform apps with Ionic",
    "version": "0.0.1",
    "description": "Ionic boilerplate to create cross platform apps (ios, android, mobile, web and desktop). Written in TypeScript and Powered by Webpack!",
    "main": "lib/index.js",
    "scripts": {
      "dumpdev": "rm -rf www/* && webpack",
      "dumpprod": "rm -rf www/* && webpack --release --config webpack.config.prod.js",
      "watch": "rm -rf www/* && webpack --watch --progress --colors",
      "browser": "npm run dumpdev && cordova run browser",
      "desktop": "npm run dumpdev && cordova build browser && electron ./platforms/browser/www/index.desktop.js",
      "android": "npm run dumpdev && cordova run android",
      "iosEmulator": "npm run dumpdev && cordova run ios",
      "ios": "npm run dumpdev && cordova run ios --device"
    },
