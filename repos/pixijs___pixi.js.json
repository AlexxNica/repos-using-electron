  "packageJSON": {
    "name": "pixi.js",
    "version": "4.1.1",
    "description": "Pixi.js is a fast lightweight 2D library that works across all devices.",
    "contributors": [
      "Ivan Popelyshev <ivan.popelyshev@gmail.com>",
      "Matt Karl <matt@mattkarl.com>",
