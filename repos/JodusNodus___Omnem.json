    "name": "omnem",
    "version": "0.6.1",
    "description": "A minimalistic text editor for writing",
    "main": "./app/app.js",
    "scripts": {
      "start": "electron .",
      "test": "electron ./tests/index.js",
      "watch": "webpack --watch",
      "build": "webpack && electron-packager ./ 'Omnem' --platform=darwin --ignore='node_modules' --ignore='dist' --ignore='graphics' --ignore='app/view/(jsx|less)' --arch=x64 --build-version=0.5 --version=0.36.0 --out=./dist --icon=./graphics/icon.icns --overwrite"
    },
