  "packageJSON": {
    "name": "keytar",
    "description": "Bindings to native Mac/Linux/Windows password APIs",
    "version": "3.0.0",
    "licenses": [
      {
        "type": "MIT",
        "url": "http://github.com/atom/node-keytar/raw/master/LICENSE.md"
      }
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/mongodb-js/node-keytar.git"
    },
    "bugs": {
      "url": "https://github.com/atom/node-keytar/issues"
    },
    "homepage": "http://atom.github.io/node-keytar",
    "keywords": [
      "keychain",
      "password",
      "passwords",
      "credential",
      "credentials",
      "vault",
      "credential vault"
    ],
    "main": "index.js",
    "binary": {
      "module_name": "keytar",
      "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
      "remote_path": "./js/{module_name}/v{version}/{configuration}/",
      "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
      "host": "https://mongodb-dx-public.s3.amazonaws.com"
    },
    "scripts": {
      "fmt": "mongodb-js-fmt",
      "check-js": "mongodb-js-precommit",
      "check-cpp": "node-cpplint src/*.cc --verbose",
      "check": "npm run check-cpp && npm run check-js",
      "preinstall": "npm install node-pre-gyp",
      "install": "node-pre-gyp install --fallback-to-build",
      "build-electron": "cross-env HOME=./.electron-gyp npm_config_disturl=https://atom.io/download/atom-shell node-pre-gyp rebuild package --target=1.2.8 --runtime=electron",
      "pretest-node": "node-pre-gyp rebuild",
      "test-node": "mocha",
      "pretest-electron": "npm run build-electron",
      "test-electron": "xvfb-maybe electron-mocha",
      "test": "npm run test-node && npm run test-electron",
      "ci": "npm test",
      "publish-node": "node-pre-gyp build package publish info --loglevel info",
      "publish-electron": "cross-env HOME=./.electron-gyp npm_config_disturl=https://atom.io/download/atom-shell node-pre-gyp build package publish info --target=1.2.8 --runtime=electron --loglevel info"
    },
    "devDependencies": {
      "aws-sdk": "^2.2.44",
      "cross-env": "^1.0.7",
      "electron-mocha": "^2.3.1",
      "electron-prebuilt": "^1.2.8",
      "eslint-config-mongodb-js": "^1.0.6",
      "mocha": "^2.3.4",
      "mongodb-js-fmt": "0.0.3",
      "mongodb-js-precommit": "^0.2.8",
      "node-cpplint": "~0.1.5",
      "rimraf": "~2.1.4",
      "xvfb-maybe": "^0.1.3"
    },
    "dependencies": {
      "debug": "^2.2.0",
      "nan": "^2.2.0",
      "node-pre-gyp": "0.x"
    },
    "bundledDependencies": [
      "node-pre-gyp"
    ]
  },
