  "packageJSON": {
    "name": "loopback-webpack-typescript-amino",
    "version": "0.0.3",
    "description": "A webpack starter for a Loopback server and Angular2 client",
    "main": "server/server.js",
    "scripts": {
      "rimraf": "rimraf",
      "tslint": "tslint",
      "typedoc": "typedoc",
      "webpack": "webpack",
      "webpack-dev-server": "webpack-dev-server",
      "webdriver-manager": "webdriver-manager",
      "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
      "clean:dist": "npm run rimraf -- dist",
      "preclean:install": "npm run clean",
      "clean:install": "npm set progress=false && npm install",
      "preclean:start": "npm run clean",
      "clean:start": "npm start",
      "watch": "npm run watch:dev",
      "watch:dev": "npm run build:dev -- --watch",
      "watch:dev:hmr": "npm run watch:dev -- --hot",
      "watch:prod": "npm run build:prod -- --watch",
      "build": "npm run build:dev",
      "prebuild:dev": "npm run clean:dist",
      "build:dev": "gulp && webpack --config client/config/webpack.dev.js --progress --profile",
      "prebuild:prod": "npm run clean:dist",
      "build:prod": "webpack --config client/config/webpack.prod.js  --progress --profile --bail",
      "server": "npm run server:dev",
      "server:dev": "webpack-dev-server --config client/config/webpack.dev.js --progress --profile --watch --content-base src/",
      "server:dev:hmr": "npm run server:dev -- --inline --hot",
      "server:prod": "http-server dist --cors",
      "webdriver:update": "npm run webdriver-manager update",
      "webdriver:start": "npm run webdriver-manager start",
      "lint": "npm run tslint \"src/**/*.ts\"",
      "pree2e": "npm run webdriver:update -- --standalone",
      "pretest": "npm run lint",
      "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
      "gh-pages": "wintersmith build -C docs && gh-pages -d docs/build",
      "start": "npm run server:dev",
      "start:hmr": "npm run server:dev:hmr",
      "version": "npm run build",
      "postversion": "git push && git push --tags",
      "postinstall": "cd client && typings install",
      "electron": "electron ./src/electron/main.js"
    },
    "keywords": [
      "loopback",
      "typescript",
      "amino",
      "webpack",
      "angular2",
      "ng2-admin"
    ],
