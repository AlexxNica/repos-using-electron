  "packageJSON": {
    "name": "WhatsBar",
    "version": "2.0.0",
    "main": "app/main.js",
    "license": "MIT",
    "author": "fscherwi",
    "scripts": {
      "postinstall": "rm ./node_modules/menubar/example/IconTemplate.png && cp ./icons/icon.png ./node_modules/menubar/example/IconTemplate.png",
      "build:mac": "electron-packager ./app WhatsBar --platform=darwin --arch=x64 --app-copyright=fscherwi --app-version=2.0.0 --asar --icon=./icons/icon.icns --overwrite --out=./build --version=1.4.0",
      "clean:mac": "rm ./build/WhatsBar-darwin-x64/LICENSE ./build/WhatsBar-darwin-x64/version ./build/WhatsBar-darwin-x64/LICENSES.chromium.html && cp LICENSE ./build/WhatsBar-darwin-x64/LICENSE",
      "prepare:mac": "npm run build:mac && npm run clean:mac",
      "build:win": "electron-packager ./app WhatsBar --platform=win32 --arch=all --app-copyright=fscherwi --app-version=2.0.0 --asar=true --icon=./icons/icon.icns --overwrite=true --out=./build  --version=1.4.0",
      "clean:win": "rm ./build/WhatsBar-win32-ia32/LICENSE ./build/WhatsBar-win32-ia32/LICENSES.chromium.html ./build/WhatsBar-win32-ia32/version ./build/WhatsBar-win32-x64/LICENSE ./build/WhatsBar-win32-x64/LICENSES.chromium.html ./build/WhatsBar-win32-x64/version && cp LICENSE ./build/WhatsBar-win32-ia32/LICENSE && cp LICENSE ./build/WhatsBar-win32-x64/LICENSE",
      "prepare:win": "npm run build:win && npm run clean:win",
      "prepare": "npm run prepare:mac && npm run prepare:win",
      "start": "electron ./app"
    },
    "dependencies": {
      "get-user-agent": "^1.6.1",
      "menubar": "^5.1.0"
    },
    "devDependencies": {
      "electron-packager": "^8.0.0",
      "electron-prebuilt": "^1.4.0"
    },
    "private": true,
    "homepage": "https://github.com/fscherwi/WhatsBar#readme"
  },
  "packageStatus": 200
