  "packageJSON": {
    "name": "sampleelectron",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
      "webpack-watch": "./node_modules/.bin/webpack -w --env=dev",
      "hot-server": "node server.js",
      "start-hot": "./node_modules/.bin/cross-env HOT=1 NODE_ENV=dev electron ./",
      "dev": "./node_modules/.bin/concurrently --kill-others \"npm run webpack-watch\" \"npm run hot-server\" \"npm run start-hot\"",
      "prod": "./node_modules/.bin/webpack --display-error-details --env=prod",
      "pack": "electron-packager . --out=dist --app-version=$npm_package_version --prune --asar --overwrite --all --ignore=\"node_modules/*\" --ignore=\"src/*\" --ignore=\".idea/*\" --ignore=\"config/*\" --ignore=\".eslintrc\" --ignore=\".gitignore\" --ignore=\".sqlite3\" --ignore=\"README.md\" --ignore=\"server.js\" --ignore=\"webpack.config.js\" --ignore=\".log\"",
      "pack-mac": "electron-packager . --out=dist --app-version=$npm_package_version --overwrite --platform=darwin --arch=x64 --ignore=\"src/*\" --ignore=\".idea/*\" --ignore=\"config/*\" --ignore=\"README.md\" --ignore=\"server.js\" --ignore=\"webpack.config.js\" --ignore=\"dev.sqlite3\"",
      "pack-test": "rm -rf dist/ && npm run prod && npm run pack-mac"
    },
    "bin": {
      "electron": "./node_modules/.bin/electron"
    },
    "devDependencies": {
      "autoprefixer": "^6.3.6",
