  "name": "youwatch",
  "productName": "YouWatch",
  "version": "0.2.1",
  "description": "An improved YouTube desktop client",
  "license": "MIT",
  "repository": "YannBertrand/YouWatch",
  "main": "dist/app.js",
  "author": {
    "name": "Yann Bertrand",
    "email": "yann@bertrand.com",
    "url": "yann-bertrand.fr"
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "1.1.0",
  "scripts": {
    "start": "gulp",
    "test": "xo src/*.js",
    "clean": "rm -rf dist; rm -f ~/.config/configstore/YouWatch.json; rm -rf ~/Library/Application\\ Support/YouWatch",
    "build": "npm run build:osx && npm run build:linux && npm run build:windows",
    "build:osx": "electron-packager . --overwrite --asar --out=build --ignore='^/(build|src|media)$' --prune --platform=darwin --arch=x64 --app-version=$npm_package_version && cd build/YouWatch-darwin-x64 && zip -ryXq9 ../YouWatch-osx-${npm_package_version}.zip YouWatch.app && cd ../ && rm -r YouWatch-darwin-x64",
    "build:linux": "electron-packager . --overwrite --out=build --ignore='^/(build|src|media)$' --prune --platform=linux --arch=x64 --app-version=$npm_package_version && cd build/YouWatch-linux-x64/ && zip -ryq9 ../YouWatch-linux-${npm_package_version}.zip * && cd ../ && rm -r YouWatch-linux-x64",
    "build:windows": "electron-packager . --overwrite --asar --out=build --ignore='^/(build|src|media)$' --prune --platform=win32 --arch=ia32 --version-string.ProductName=$npm_package_productName --app-version=$npm_package_version && cd build/YouWatch-win32-ia32 && zip -ryq9 ../YouWatch-windows-${npm_package_version}.zip * && cd ../ && rm -r YouWatch-win32-ia32"
  },
