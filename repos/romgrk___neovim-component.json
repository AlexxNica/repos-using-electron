  "packageJSON": {
    "name": "neovim-component",
    "version": "0.5.3",
    "description": "Polymer component for Neovim frontend",
    "main": "example/minimal/main.js",
    "bin": {
      "nvim-md-example": "./example/minimal/cli.js"
    },
    "repository": {
      "user": "rhysd",
      "repo": "neovim-component",
      "host": "github.com",
      "branch": "master",
      "apiHost": "api.github.com",
      "tarball_url": "https://api.github.com/repos/rhysd/neovim-component/tarball/master",
      "clone_url": "https://github.com/rhysd/neovim-component",
      "https_url": "https://github.com/rhysd/neovim-component",
      "travis_url": "https://travis-ci.org/rhysd/neovim-component",
      "zip_url": "https://github.com/rhysd/neovim-component/archive/master.zip",
      "api_url": "https://api.github.com/repos/rhysd/neovim-component"
    },
    "scripts": {
      "all": "npm run dep && npm run build && npm run example",
      "build": "tsc -p src && browserify -o build/index.js src/out/index.js",
      "debug": "NODE_ENV=debug electron .",
      "dep": "npm install && bower install && typings install && mkdir -p $(npm prefix)/build",
      "example": "electron .",
      "lint": "tslint $(git ls-files | grep -E \"\\.ts$\")",
      "watch": "cd \"$(npm prefix)\" && guard --watchdir src test",
      "test": "cd \"$(npm prefix)\" && mocha test/unit/"
    },
    "keywords": [
      "neovim",
      "polymer",
      "WebComponent",
      "Electron",
      "NW.js",
      "editor"
    ],
    "author": "rhysd <lin90162@yahoo.co.jp>",
    "license": "MIT",
    "dependencies": {
      "flux": "^2.1.1",
      "loglevel": "^1.4.0",
      "promised-neovim-client": "^1.0.1"
    },
    "devDependencies": {
      "bower": "^1.7.9",
      "browserify": "^13.0.0",
      "canvas": "^1.3.12",
      "chai": "^3.5.0",
      "electron-prebuilt": "^0.37.7",
      "jsdom": "^8.4.0",
      "mocha": "^2.4.5",
      "mocha-generators": "^1.2.0",
      "nightmare": "https://github.com/rhysd/nightmare.git#custom-electron-path",
      "tslint": "^3.8.0",
      "typescript": "^1.8.10",
      "typings": "^0.7.1"
    }
  },
