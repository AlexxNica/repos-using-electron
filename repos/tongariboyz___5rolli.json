    "scripts": {
      "app": "electron --debug=5858 dist/main.js",
      "build": "npm-run-all --parallel build:*",
      "build:css": "stylus -c src/styles/base.styl --include-css -I node_modules -o dist/bundle.css -m --sourcemap-root stylus",
      "build:fonts": "cpx \"src/fonts/**/{*.eot,*.svg,*.ttf,*.woff}\" dist/fonts",
      "build:images": "cpx \"src/images/**/{*.svg,*.gif,*.png,*.jpg}\" dist/images",
      "build:jade": "jade src --out dist",
      "build:js-main": "browserify -t babelify src/js/main/main.js --im --no-detect-globals --node -d | exorcist ./dist/main.js.map > ./dist/main.js",
      "build:js-renderer": "browserify -t babelify src/js/renderer/app.js -d | exorcist ./dist/bundle.js.map > ./dist/bundle.js",
      "codecov": "cat coverage/lcov.info | codecov",
      "deploy": "npm-run-all doc gh-pages",
      "doc": "esdoc -c esdoc.json",
      "gh-pages": "gh-pages -d doc",
      "lint": "eslint --cache {src/js,test}",
      "prebuild": "rm -rf dist && mkdir -p dist",
      "predoc": "rm -rf doc",
      "prerelease": "rm -rf build/src && mkdir -p build/src",
      "pretest": "rm -rf coverage",
      "prewatch": "mkdir -p {dist,build}",
      "release": "npm-run-all -s release:build release:pack-osx",
      "release:build": "npm-run-all -s release:clean release:copy -p release:css release:js-main release:js-renderer release:jade",
      "release:copy": "npm-run-all -p release:package release:fonts release:images",
      "release:css": "stylus -c src/styles/base.styl -I node_modules -o build/src/bundle.css",
      "release:fonts": "cpx \"src/fonts/**/{*.eot,*.svg,*.ttf,*.woff}\" build/src/fonts",
      "release:images": "cpx \"src/images/**/{*.svg,*.gif,*.png,*.jpg}\" build/src/images",
      "release:jade": "jade src --out build/src",
      "release:js-main": "browserify -t babelify ./src/js/main/main.js --im --no-detect-globals --node | uglifyjs -c warnings=false -d DEBUG=false > ./build/src/main.js",
      "release:js-renderer": "browserify -t babelify ./src/js/renderer/app.js | uglifyjs -c warnings=false -d DEBUG=false > ./build/src/bundle.js",
      "release:pack-osx": "node scripts/build.js --platform=darwin",
      "release:package": "cpx ./package.json ./build/src",
      "start": "npm run watch",
      "stop": "npm-run-all stop:*",
      "stop:flow": "flow stop",
      "test": "npm-run-all lint test:coverage",
      "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- --timeout=10000 test/**/*spec.js",
      "watch": "npm-run-all --parallel watch:*",
      "watch:css": "stylus -c -w src/styles/base.styl --include-css -I node_modules -o dist/bundle.css -m --sourcemap-root ./stylus",
      "watch:flow": "flow & chokidar src/js/ -c flow",
      "watch:fonts": "cpx \"src/fonts/**/{*.eot,*.svg,*.ttf,*.woff}\" dist/fonts --watch",
      "watch:images": "cpx \"src/images/**/{*.svg,*.gif,*.png,*.jpg}\" dist/images --watch",
      "watch:jade": "jade src --watch --out dist",
      "watch:js-main": "watchify -v -t babelify ./src/js/main/main.js --im --no-detect-globals --node -o \"exorcist ./dist/main.js.map > ./dist/main.js\" -d",
      "watch:js-renderer": "watchify -v -t babelify src/js/renderer/app.js -o \"exorcist dist/bundle.js.map > ./dist/bundle.js\" -d",
      "watch:lint": "chokidar src/js test -c 'npm run -s lint'",
      "watch:server": "node-inspector",
      "watch:test": "mocha --timeout=10000 --watch test/**/*spec.js"
    },
