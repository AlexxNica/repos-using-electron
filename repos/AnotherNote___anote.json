  "packageJSON": {
    "name": "Anote",
    "version": "1.0.0",
    "description": "a markdown note based on electron",
    "productName": "Anote",
    "main": "index.js",
    "scripts": {
      "build_js": "babel --quiet src --out-dir build",
      "build": "babel --quiet src --out-dir build && electron-packager . Anote --platform=darwin --arch=x64 --icon='./static/images/anote.icns' --ignore=node_modules/electron-*",
      "start": "npm run build_js && electron .",
      "develop": "npm run build_js && (babel src --out-dir build --watch & electron .)",
      "package": "asar pack Anote-darwin-x64/Anote.app/Contents/Resources/app Anote-darwin-x64/Anote.app/Contents/Resources/app.asar && rm -rf Anote-darwin-x64/Anote.app/Contents/Resources/app"
    },
    "repository": {
