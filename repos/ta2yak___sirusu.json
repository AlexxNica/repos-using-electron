    "name": "Sirusu",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "start": "gulp && electron .",
      "test": "echo \"Warning: no test specified\"",
      "clean": "rm -rf ./release",
      "clean:osx": "rm -rf ./release/osx",
      "clean:win": "rm -rf ./release/win",
      "pack": "npm-run-all -p pack:osx pack:win",
      "pack:osx": "npm run clean:osx && electron-packager . \"Sirusu\" --out=release/osx --platform=darwin --arch=x64 --version=0.36.7 --icon=assets/osx/icon.icns --ignore=\"release|assets|app\"",
      "pack:win": "npm run clean:win && electron-packager . \"Sirusu\" --out=release/win --platform=win32 --arch=ia32 --version=0.36.7 --icon=assets/win/icon.ico --ignore=\"release|assets|app\"",
      "build": "npm-run-all -p build:osx build:win",
      "build:osx": "npm run pack:osx && electron-builder \"release/osx/Sirusu-darwin-x64/Sirusu.app\" --platform=osx --out=\"release/osx\" --config=builder.json",
      "build:win": "npm run pack:win && electron-builder \"release/win/Sirusu-win32-ia32\" --platform=win --out=\"release/win\" --config=builder.json",
      "archive": "npm-run-all -p archive:osx archive:win",
      "archive:osx": "npm run build:osx && zip -j \"release/osx/Sirusu-osx.zip\" \"release/osx/Sirusu.dmg\"",
      "archive:win": "npm run build:win && zip -j \"release/win/Sirusu-win.zip\" \"release/win/Sirusu Setup.exe\""
    },
