  "packageJSON": {
    "name": "flying-illusion",
    "version": "0.1.5",
    "description": "Dance game for the Flying Illusion show",
    "scripts": {
      "dev": "node tasks/server-dev.es5.js",
      "build": "webpack --colors --progress --config tasks/webpack.config.prod.babel.js",
      "prod": "npm run build && node tasks/server-prod.es5.js",
      "deploy": "git subtree push --prefix www origin gh-pages",
      "electron": "rm -rf ./www/electron/* && cp -r ./src/electron ./www",
      "build:electron": "npm run build && npm run electron",
      "rebuild": "electron-rebuild $(electron -v) -p -f -m www -w serialport -e node_modules/electron-prebuilt",
      "start": "npm run electron && electron www",
      "develectron": "rm -rf ./www/electron/* && cp -r ./src/electron ./www && npm run start",
      "app:osx": "electron-packager www flying-illusion --platform=darwin --arch=x64 --out=electron/osx --version=0.37.2 --overwrite",
      "app:linux": "electron-packager www flying-illusion --platform=linux --arch=x64 --out=electron/linux --version=0.37.2 --overwrite",
      "app:windows": "electron-packager www flying-illusion --platform=win32 --arch=ia32 --out=electron/win --version=0.37.2 --overwrite",
      "pack:linux": "node tasks/buildPackage.es5.js",
      "pack:osx": "electron-builder electron/osx/flying-illusion-darwin-x64/flying-illusion.app --platform=osx --out=electron/osx --config=resources/config.json"
    },
    "repository": {
      "type": "git",
