  "packageJSON": {
    "name": "kira-takip",
    "version": "1.0.0",
    "description": "Herkod Yazılım tarafından geliştirilen kira-takip yazılımıdır.",
    "repository": "https://github.com/bytheway/electron-sqlite3.git",
    "main": "main.js",
    "dependencies": {
      "jquery": "^2.1.4",
      "sqlite3": "^3.1.4"
    },
    "devDependencies": {
      "electron-prebuilt": "^1.2.5",
      "electron-packager": "^7.0.4"
    },
    "keywords": [
      "electron",
      "atom",
      "sqlite3"
    ],
    "scripts": {
      "start": "electron .",
      "build": "electron-packager . electron-sqlite3 --out=dist --ignore=dist --prune --asar --all --version=1.2.5",
      "postinstall": "npm run rebuild-sqlite3-win",
      "rebuild-sqlite3-win": "cd node_modules/sqlite3 && npm run prepublish && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.2-win32-x64 && node-gyp node-gyp rebuild --target=1.2.5 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.2-win32-x64"
    },
    "contributors": [],
